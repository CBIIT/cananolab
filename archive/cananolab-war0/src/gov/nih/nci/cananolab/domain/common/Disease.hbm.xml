<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Disease" table="disease" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="disease_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Disease class -->
		<property name="conceptCode" type="string" column="concept_code"/>
		<property name="name" type="string" column="disease_name"/>
		<property name="type" type="string" column="disease_type"/>
		<!-- Associations mapping for the Disease class -->
		<set name="studyCollection" table="study_disease" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="disease_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Study" column="study_pk_id" />
		</set>
	</class>
</hibernate-mapping>
