<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
	
		<!-- Properties for the session factory -->
		<property name="connection.datasource">java:/caNanoLab</property>
		<property name="hibernate.show_sql">false</property>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="max_fetch_depth">1</property>
		<property name="cglib.use_reflection_optimizer">false</property>
		<property name="jdbc.batch_versioned_data">true</property>
		<property name="jdbc.use_streams_for_binary">true</property>
		<property name="cache.use_query_cache">false</property>
		<property name="cache.provider_class">org.hibernate.cache.EhCacheProvider</property>

		<!-- Mapped Resources -->
		<mapping resource="gov/nih/nci/cananolab/domain/characterization/invivo/AnatomicSite.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/DoseComponent.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Material.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Sample.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Study.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/ActivationMethod.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Animal.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Keyword.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Instrument.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Condition.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Technique.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/TreatmentGroup.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Datum.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/AnimalDiet.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Dose.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Author.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Experiment.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/AnimalModel.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Organization.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Treatment.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/CommonLookup.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Function.hbm.xml" />
