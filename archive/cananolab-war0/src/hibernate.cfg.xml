		<mapping resource="gov/nih/nci/cananolab/domain/common/Protocol.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/MaterialComponent.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/ExperimentConfig.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/ChemicalAssociation.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/File.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Disease.hbm.xml" />
		<mapping resource="gov/nih/nci/cananolab/domain/common/Organism.hbm.xml" />
