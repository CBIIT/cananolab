<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Study" table="study" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="study_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Study class -->
		<property name="analysisConclusion" type="string" column="analysis_conclusion"/>
		<property name="animalStudy" type="boolean" column="is_animal_study"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="designMethodsDescription" type="string" column="design_method_description"/>
		<property name="endDate" type="java.util.Date" column="end_date"/>
		<property name="name" type="string" column="study_name"/>
		<property name="startDate" type="java.util.Date" column="start_date"/>
		<property name="type" type="string" column="study_type"/>
		<!-- Associations mapping for the Study class -->
		<set name="animalModelCollection" table="study_animal_model" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.AnimalModel" column="animal_model_pk_id" />
		</set>
		<set name="publicationCollection" table="study_publication" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Publication" column="publication_pk_id" />
		</set>
		<many-to-one name="primaryPointOfContact" class="gov.nih.nci.cananolab.domain.common.PointOfContact" column="primary_contact_pk_id" lazy="proxy" />
		<set name="otherPointOfContactCollection" table="study_other_poc" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.PointOfContact" column="poc_pk_id" />
		</set>
		<set name="sampleCollection" table="study_sample" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Sample" column="sample_pk_id" />
		</set>
		<set name="protocolCollection" table="study_protocol" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Protocol" column="protocol_pk_id" />
		</set>
		<set name="diseaseCollection" table="study_disease" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="study_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Disease" column="disease_pk_id" />
		</set>
	</class>
</hibernate-mapping>
