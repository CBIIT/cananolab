<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Protocol" table="Protocol" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="PROTOCOL_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="name" type="java.lang.String" column="PROTOCOL_NAME" />
    <set name="protocolFileCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.ProtocolFile" />
    </set>
    <set name="assayCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="PROTOCOL_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.Assay" />
    </set>
  </class>
</hibernate-mapping>

