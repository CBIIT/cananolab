<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="LabFile" table="Lab_File" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="FILE_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="uri" type="java.lang.String" column="FILE_URI" />
    <property name="extension" type="java.lang.String" column="FILE_TYPE_EXTENSION" />
    <property name="createdBy" type="java.lang.String" column="CREATED_BY" />
    <property name="createdDate" type="java.util.Date" column="CREATED_DATE" />
    <property name="version" type="java.lang.String" column="VERSION" />
    <joined-subclass name="gov.nih.nci.calab.domain.OutputFile" table="View_Run_Output_File" lazy="true">
      <key column="OUTPUT_FILE_PK_ID" />
      <many-to-one name="run" class="gov.nih.nci.calab.domain.Run" column="RUN_PK_ID" lazy="true" />
    </joined-subclass>
    <joined-subclass name="gov.nih.nci.calab.domain.SampleSOPFile" table="View_Sample_SOP_File" lazy="true">
      <key column="SOP_FILE_PK_ID" />
      <many-to-one name="sampleSOP" class="gov.nih.nci.calab.domain.SampleSOP" column="SAMPLE_SOP_PK_ID" lazy="true" />
    </joined-subclass>
    <joined-subclass name="gov.nih.nci.calab.domain.InputFile" table="View_Run_Input_File" lazy="true">
      <key column="INPUT_FILE_PK_ID" />
      <many-to-one name="run" class="gov.nih.nci.calab.domain.Run" column="RUN_PK_ID" lazy="true" />
    </joined-subclass>
    <joined-subclass name="gov.nih.nci.calab.domain.ProtocolFile" table="View_Protocol_File" lazy="true">
      <key column="PROTOCOL_FILE_PK_ID" />
      <many-to-one name="protocol" class="gov.nih.nci.calab.domain.Protocol" column="PROTOCOL_PK_ID" lazy="true" />
    </joined-subclass>
  </class>
</hibernate-mapping>

