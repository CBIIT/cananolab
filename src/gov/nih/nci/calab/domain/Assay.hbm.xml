<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Assay" table="Assay" lazy="true" polymorphism="explicit" >
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ASSAY_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="ASSAY_NAME" />
    <property name="createdBy" type="java.lang.String" column="CREATED_BY" />
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="assayType" type="java.lang.String" column="ASSAY_TYPE" />
    <property name="createdDate" type="java.util.Date" column="CREATED_DATE" />
    <many-to-one name="protocol" class="gov.nih.nci.calab.domain.Protocol" column="PROTOCOL_PK_ID"/>
    <set name="runCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="ASSAY_PK_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.Run" />
    </set>
  </class>
</hibernate-mapping>

