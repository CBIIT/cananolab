<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="User" table="CSM_USER" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="USER_ID">
      <generator class="assigned" />
    </id>
    <property name="firstName" type="java.lang.String" column="FIRST_NAME" />
    <property name="lastName" type="java.lang.String" column="LAST_NAME" />
    <property name="title" type="java.lang.String" column="TITLE" />
    <property name="phoneNumber" type="java.lang.String" column="PHONE_NUMBER" />
    <property name="email" type="java.lang.String" column="EMAIL_ID" />
    <property name="updateDate" type="java.util.Date" column="UPDATE_DATE" />
    <property name="startDate" type="java.util.Date" column="START_DATE" />
    <property name="endDate" type="java.util.Date" column="END_DATE" />
    <property name="department" type="java.lang.String" column="DEPARTMENT" />
    <property name="organization" type="java.lang.String" column="ORGANIZATION" />
    <property name="loginName" type="java.lang.String" column="LOGIN_NAME" />
  </class>
</hibernate-mapping>
