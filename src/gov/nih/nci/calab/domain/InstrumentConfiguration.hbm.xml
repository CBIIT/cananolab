<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
	<class name="InstrumentConfiguration" table="instrument_config"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long"
			column="instrument_config_pk_id" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="description" type="java.lang.String"
			column="description" />
		<many-to-one name="instrument"
			class="gov.nih.nci.calab.domain.Instrument" column="instrument_pk_id"
			lazy="false" fetch="join" />
	</class>
</hibernate-mapping>
