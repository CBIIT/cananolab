<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.function">
	<class name="AgentTarget" table="agent_target" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="agent_target_pk_id" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<discriminator column="discriminator" type="java.lang.String" />
		<property name="description" type="java.lang.String" column="description" />
		
		<subclass name="UnclassifiedAgentTarget" discriminator-value="Other" />
		
		<subclass name="Receptor" discriminator-value="Receptor">
			<property name="name" column="name" />
		</subclass>

		<subclass name="Antigen" discriminator-value="Antigen">
			<property name="name" column="name" />
		</subclass>
	</class>
</hibernate-mapping>
