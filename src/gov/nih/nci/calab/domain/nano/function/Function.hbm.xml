<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.function">
	<class name="Function" table="PARTICLE_FUNCTION" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="PARTICLE_FUNCTION_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>

		<property name="type" type="java.lang.String" column="TYPE" />
		<property name="activationMethod" type="java.lang.String" column="ACTIVATION_METHOD" />
		<property name="description" type="java.lang.String" column="DESCRIPTION" />
		<property name="identificationName" type="java.lang.String" column="IDENTIFIER_NAME" />

		<list name="linkageCollection" cascade="all-delete-orphan" lazy="true">
			<cache usage="read-write" />
			<key column="FUNCTION_PK_ID" />
			<list-index column="LIST_INDEX" />
			<one-to-many class="gov.nih.nci.calab.domain.nano.function.Linkage" />
		</list>
	</class>
</hibernate-mapping>


