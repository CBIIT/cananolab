<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	package="gov.nih.nci.calab.domain.nano.characterization.physical.composition">
	<class name="ComposingElementType"
		table="DEF_composing_element_Type" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long"
			column="composing_element_TYPE_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="NAME" />
	</class>
	<class name="MolecularFormulaType"
		table="DEF_Molecular_Formula_Type" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long"
			column="Molecular_Formula_TYPE_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="NAME" />
	</class>
	<class name="SurfaceGroupType" table="DEF_Surface_Group_Type"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long"
			column="SURFACE_GROUP_TYPE_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="NAME" />
	</class>
</hibernate-mapping>
