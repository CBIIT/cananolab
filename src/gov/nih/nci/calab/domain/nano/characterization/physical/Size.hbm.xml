<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.physical">
 <subclass name="Shape" discriminator-value="size" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization">
    	<join table="PHYSICAL_SIZE">
    		<key column="CHARACTERIZATION_PK_ID" />
    		<property name="type" column="TYPE" type="java.lang.String" />
    		<property name="maxDimension" column="SIZE_DISTRIBUTION" type="java.lang.String" />
    		<property name="minDimension" column="RADIUS_MOMENTS" type="java.lang.String" />
    		<property name="polydispersityIndex" column="POLYDISPERSITY_INDEX" type="java.lang.String" />
	   	</join>	   	
   </subclass>   
</hibernate-mapping> 