<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.physical">
	<subclass name="Size" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Size" />

	<subclass name="Stability" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Stability">
		<join table="STABILITY">
			<key column="STABILITY_PK_ID" />
			<component name="longTermStorage" class="gov.nih.nci.calab.domain.Measurement">
				<property name="value" column="LONG_TERM_STORAGE" />
				<property name="unitOfMeasurement" column="LONG_TERM_STORAGE_UNIT" />
			</component>
			<component name="shortTermStorage" class="gov.nih.nci.calab.domain.Measurement">
				<property name="value" column="SHORT_TERM_STORAGE" />
				<property name="unitOfMeasurement" column="SHORT_TERM_STORAGE_UNIT" />
			</component>
			<property name="stressResult" type="java.lang.String" column="STRESS_RESULT" />
			<property name="releaseKineticsDescription" type="java.lang.String" column="RELEASE_KINETICS_DESCRIPTION" />
			<property name="measurementType" type="java.lang.String" column="MEASUREMENT_TYPE" />
			<component name="stressor" class="gov.nih.nci.calab.domain.nano.characterization.physical.Stressor">
				<property name="type" column="STRESSOR_TYPE" />
				<property name="description" column="STRESSOR_DESC" />
				<component name="value" class="gov.nih.nci.calab.domain.Measurement">
					<property name="value" column="STRESSOR_VALUE" />
					<property name="unitOfMeasurement" column="STRESSOR_VALUE_UNIT" />
				</component>
			</component>
		</join>
	</subclass>
</hibernate-mapping>

