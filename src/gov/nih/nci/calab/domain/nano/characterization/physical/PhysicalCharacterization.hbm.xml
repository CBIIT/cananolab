<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.physical">
	<subclass name="Size" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Size" />


	<subclass name="MolecularWeight" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="MolecularWeight" />
	<subclass name="Morphology" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Morphology">
		<join table="MORPHOLOGY">
			<key column="MORPHOLOGY_PK_ID" />
			<property name="type" type="java.lang.String" column="TYPE" />
		</join>
	</subclass>
	<subclass name="Shape" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Shape">
		<join table="SHAPE">
			<key column="SHAPE_PK_ID" />
			<property name="type" type="java.lang.String" column="TYPE" />
			<property name="maxDimension" type="java.lang.Float" column="MAX_DIMENSION" />
			<property name="minDimension" type="java.lang.Float" column="MIN_DIMENSION" />
		</join>
	</subclass>
	<subclass name="Solubility" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Solubility">
		<join table="SOLUBILITY">
			<key column="SOLUBILITY_PK_ID" />
			<property name="solvent" type="java.lang.String" column="SOLVENT" />
			<property name="isSoluble" type="java.lang.Boolean" column="IS_SOLUBLE" />
			<component name="criticalConcentration" class="gov.nih.nci.calab.domain.Measurement">
				<property name="value" column="CRITICAL_CONCENTRATION" />
				<property name="unitOfMeasurement" column="CONCENTRATION_UNIT" />
			</component>
		</join>
	</subclass>
	<subclass name="Surface" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Surface">
		<list name="surfaceChemistryCollection" cascade="all-delete-orphan" lazy="false">
				<cache usage="read-write" />
				<key column="SURFACE_PK_ID" />
				<list-index column="LIST_INDEX" /> 
				<one-to-many class="gov.nih.nci.calab.domain.nano.characterization.physical.SurfaceChemistry" />
			</list>
		<join table="SURFACE">
			<key column="SURFACE_PK_ID" />
			<component name="surfaceArea" class="gov.nih.nci.calab.domain.Measurement">
				<property name="value" column="SURFACE_AREA" />
				<property name="unitOfMeasurement" column="SURFACE_AREA_UNIT" />
			</component>
			<component name="charge" class="gov.nih.nci.calab.domain.Measurement">
				<property name="value" column="CHARGE" />
				<property name="unitOfMeasurement" column="CHARGE_UNIT" />
			</component>
			<property name="isHydrophobic" type="java.lang.Boolean" column="IS_HYDROPHOBIC" />
			<property name="zetaPotential" type="java.lang.Float" column="ZETA_POTENTIAL" />
		</join>
	</subclass>
	<subclass name="Purity" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization" discriminator-value="Purity" />
</hibernate-mapping>

