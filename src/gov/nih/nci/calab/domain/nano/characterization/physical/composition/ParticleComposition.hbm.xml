<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	package="gov.nih.nci.calab.domain.nano.characterization.physical.composition">
	<subclass name="ParticleComposition"
		extends="gov.nih.nci.calab.domain.nano.characterization.Characterization"
		discriminator-value="PC">
		<list name="composingElementCollection"
			cascade="all-delete-orphan" lazy="true">
			<cache usage="read-write" />
			<key column="characterization_pk_id" />
			<list-index column="list_index" />
			<one-to-many
				class="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ComposingElement" />
		</list>
		<subclass name="QuantumDotComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="QuantumDotComp" />
		<subclass name="MetalParticleComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="MetalComp" />
		<subclass name="ComplexComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="ComplexComp" />
		<subclass name="FullereneComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="FullereneComp">
			<join table="fullerene_composition">
				<key column="f_composition_pk_id" />
				<property name="numberOfCarbon"
					column="number_of_carbon" type="java.lang.Integer" />
			</join>
		</subclass>
		<subclass name="DendrimerComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="DendrimerComp">
			<list name="surfaceGroupCollection"
				cascade="all-delete-orphan" lazy="false">
				<cache usage="read-write" />
				<key column="d_composition_pk_id" />
				<list-index column="list_index" />
				<one-to-many
					class="gov.nih.nci.calab.domain.nano.characterization.physical.composition.SurfaceGroup" />
			</list>
			<join table="dendrimer_composition">
				<key column="d_composition_pk_id" />
				<property name="branch" column="branch"
					type="java.lang.String" />
				<property name="generation" column="generation"
					type="java.lang.Float" />
				<property name="molecularFormula"
					column="molecular_formula" type="java.lang.String" />
				<property name="repeatUnit" column="repeat_unit"
					type="java.lang.String" />
			</join>
		</subclass>
		<subclass name="CarbonNanotubeComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="CarbNanotubeComp">
			<join table="carbon_nanotube_composition">
				<key column="cn_composition_pk_id" />
				<property name="chirality" column="chirality"
					type="java.lang.String" />
				<property name="growthDiameter" column="growth_diameter"
					type="java.lang.Float" />
				<property name="averageLength" column="average_length"
					type="java.lang.Float" />
				<property name="wallType" column="wall_type"
					type="java.lang.String" />
			</join>
		</subclass>
		<subclass name="LiposomeComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="LiposomeComp">
			<join table="liposome_composition">
				<key column="l_composition_pk_id" />
				<property name="polymerized" column="is_polymerized"
					type="boolean" />
				<property name="polymerName" column="polymer_name"
					type="java.lang.String" />
			</join>
		</subclass>
		<subclass name="PolymerComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="PolymerComp">
			<join table="polymer_composition">
				<key column="p_composition_pk_id" />
				<property name="crossLinked" column="is_cross_link"
					type="boolean" />
				<property name="crossLinkDegree"
					column="cross_link_degree" type="java.lang.Float" />
				<property name="initiator" column="initiator"
					type="java.lang.String" />
			</join>
		</subclass>
		<subclass name="EmulsionComposition"
			extends="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ParticleComposition"
			discriminator-value="EmulsionComp">
			<join table="emulsion_composition">
				<key column="e_composition_pk_id" />
				<property name="type" column="emulsion_type"
					type="java.lang.String" />
				<property name="molecularFormula"
					column="molecular_formula" type="java.lang.String" />
				<property name="polymerized" column="is_polymerized"
					type="boolean" />
				<property name="polymerName" column="polymer_name"
					type="java.lang.String" />
			</join>
		</subclass>
	</subclass>
</hibernate-mapping>
