<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.invitro">

	<subclass name="PlateletAggregation" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="PlateletAggregation" />
	<subclass name="Hemolysis" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="Hemolysis" />
	<subclass name="Coagulation" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="Coagulation" />
	<subclass name="PlasmaProteinBinding" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="PlasmaProteinBinding" />

	<subclass name="ComplementActivation" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="ComplementActivation" />
	<subclass name="CytokineInduction" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="CytokineInduction" />
	<subclass name="Chemotaxis" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="Chemotaxis" />
	<subclass name="NKCellCytotoxicActivity" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="NKCellCytotoxicActivity" />
	<subclass name="LeukocyteProliferation" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="LeukocyteProliferation" />
	<subclass name="OxidativeBurst" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="OxidativeBurst" />
	<subclass name="CFU_GM" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="CFU_GM" />
	<subclass name="Phagocytosis" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.ImmunoToxicity" discriminator-value="Phagocytosis" />

	<subclass name="Caspase3Activation" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.Cytotoxicity" discriminator-value="Caspase3Activation">
		<join table="CYTOTOXICITY">
			<key column="CYTOTOXICITY_PK_ID" />
			<property name="cellLine" type="java.lang.String" column="CELL_LINE" />
			<property name="cellDeathMethod" type="java.lang.String" column="CELL_DEATH_METHOD" />
		</join>
	</subclass>
	<subclass name="CellViability" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.Cytotoxicity" discriminator-value="CellViability">
		<join table="CYTOTOXICITY">
			<key column="CYTOTOXICITY_PK_ID" />
			<property name="cellLine" type="java.lang.String" column="CELL_LINE" />
			<property name="cellDeathMethod" type="java.lang.String" column="CELL_DEATH_METHOD" />
		</join>
	</subclass>
	<subclass name="EnzymeInduction" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.Toxicity" discriminator-value="EnzymeInduction" />
	<subclass name="OxidativeStress" extends="gov.nih.nci.calab.domain.nano.characterization.toxicity.Toxicity" discriminator-value="OxidativeStress" />

