<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization">
  <class name="Characterization" table="Base_Characterization" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="CHARACTERIZATION_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <discriminator column="TYPE" type="java.lang.String" />
    <property name="source" type="java.lang.String" column="ORIGIN" />
    <property name="classification" type="java.lang.String" column="CATEGORY" />
      
    <set name="particleCollection" table="PARTICLE_CHARACTERIZATION" cascade="all-delete-orphan" lazy="true" inverse="true">
		 <cache usage="read-write" />
		 <key column="CHARACTERIZATION_PK_ID" />
		 <many-to-many class="gov.nih.nci.calab.domain.nano.particle.Nanoparticle" column="PARTICLE_PK_ID"/>
    </set>
  </class>
</hibernate-mapping>
