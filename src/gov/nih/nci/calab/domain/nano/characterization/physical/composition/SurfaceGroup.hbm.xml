<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.physical.composition">
  <class name="SurfaceGroup" table="SURFACE_GROUP" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="SURFACE_GROUP_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="name" type="java.lang.String" column="NAME" />
    <property name="modifier" type="java.lang.String" column="MODIFIER" />
           
    <many-to-one name="dendrimerComposition" class="gov.nih.nci.calab.domain.nano.characterization.physical.composition.DendrimerComposition" column="D_COMPOSITION_PK_ID" lazy="false" fetch="join"/>

  </class>
</hibernate-mapping>
