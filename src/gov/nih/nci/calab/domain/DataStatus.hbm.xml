<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="DataStatus" table="Data_Status" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="DATA_STATUS_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="status" type="java.lang.String" column="STATUS" />
    <property name="reason" type="java.lang.String" column="REASON" />
  </class>
</hibernate-mapping>

