<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="AssayType" table="Assay_Type" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ASSAY_TYPE_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="ASSAY_TYPE_NAME" />
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="executeOrder" type="java.lang.String" column="EXECUTE_ORDER" />
  </class> 
</hibernate-mapping>

