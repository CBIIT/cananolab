<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
	<class name="Instrument" table="INSTRUMENT" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="INSTRUMENT_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="description" type="java.lang.String" column="DESCRIPTION" />
		<many-to-one name="manufacturer"  class="gov.nih.nci.calab.domain.Manufacturer" column="MANUFACTURER_PK_ID" lazy="false" fetch="join"/>  
		<many-to-one name="instrumentType"  class="gov.nih.nci.calab.domain.InstrumentType" column="INSTRUMENT_TYPE_PK_ID" lazy="false" fetch="join"/>  
	</class>
</hibernate-mapping>