<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Keyword" table="Keyword" lazy="true" polymorphism="explicit" >
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="KEYWORD_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="ASSAY_NAME" />
    
    <many-to-one name="nanoparticle" class="gov.nih.nci.calab.domain.nano.particle.Nanoparticle" column="PARTICLE_PK_ID" lazy="true" />
  </class>
</hibernate-mapping>

