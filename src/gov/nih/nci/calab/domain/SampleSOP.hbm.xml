<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="SampleSOP" table="Sample_SOP" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="SAMPLE_SOP_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="name" type="java.lang.String" column="SOP_NAME" />
    <set name="sampleSOPFileCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="SAMPLE_SOP_ID" />
      <one-to-many class="gov.nih.nci.calab.domain.SampleSOPFile" />
    </set>
  </class>
</hibernate-mapping>

