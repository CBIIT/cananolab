<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
   <class name="SampleType" table="def_sample_type" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="sample_type_pk_id"
			unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="name" />
	</class>
	<class name="MeasureUnit" table="def_measure_unit" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="measure_unit_pk_id"
			unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String"
			column="unit_name" />
		<property name="description" type="java.lang.String"
			column="description" />
		<property name="type" type="java.lang.String"
			column="unit_type" />
	</class>
	<class name="ProtocolType" table="def_protocol_type" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="protocol_type_pk_id"
			unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="name" />
	</class>
	<class name="MeasureType" table="def_measure_type" lazy="true"
		polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="measure_type_pk_id"
			unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="name" />
	</class>
</hibernate-mapping>
