<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="MeasureUnit" table="Measure_Unit" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="MEASURE_UNIT_PK_ID" unsaved-value="0">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="UNIT_NAME" />
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="type" type="java.lang.String" column="UNIT_TYPE" />
  </class>
</hibernate-mapping>