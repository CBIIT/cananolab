<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Storage" table="Storage" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="STORAGE_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="location" type="java.lang.String" column="STORAGE_LOCATION" />
    <property name="type" type="java.lang.String" column="STORAGE_TYPE" />
    <one-to-one name="sampleProperty" class="gov.nih.nci.calab.domain.SampleContainer" property-ref="storage" />
  </class>
</hibernate-mapping>

