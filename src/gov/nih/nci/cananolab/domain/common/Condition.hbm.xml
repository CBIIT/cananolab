<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Condition" table="experiment_condition" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="condition_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Condition class -->
		<property name="name" type="string" column="name"/>
		<property name="value" type="string" column="value"/>
		<property name="valueType" type="string" column="value_type"/>
		<property name="valueUnit" type="string" column="value_unit"/>
		<!-- Associations mapping for the Condition class -->
		<set name="datumCollection" table="datum_condition" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="condition_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Datum" column="datum_pk_id" />
		</set>
	</class>
</hibernate-mapping>
