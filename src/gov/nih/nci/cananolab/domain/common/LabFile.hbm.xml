<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="LabFile" table="lab_file" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="file_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the LabFile class -->
		<property name="comments" type="string" column="comments"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="description" type="string" column="description"/>
		<property name="extension" type="string" column="file_extension"/>
		<property name="name" type="string" column="file_name"/>
		<property name="title" type="string" column="title"/>
		<property name="type" type="string" column="file_type"/>
		<property name="uri" type="string" column="file_uri"/>
		<property name="version" type="string" column="version"/>
		<!-- Associations mapping for the LabFile class -->
		<set name="keywordCollection" table="keyword_lab_file" lazy="false" inverse="false">
			<cache usage="read-write" />
			<key column="lab_file_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Keyword" column="keyword_pk_id" />
		</set>
		<joined-subclass name="gov.nih.nci.cananolab.domain.common.Report" table="report" lazy="true">
			<key column="report_pk_id" />
			<!-- Attributes mapping for the Report class -->
			<property name="category" type="string" column="category"/>
			<!-- Associations mapping for the Report class -->
			<set name="nanoparticleSampleCollection" table="nanoparticle_sample_report" lazy="true" inverse="true">
				<cache usage="read-write" />
				<key column="particle_sample_pk_id" />
				<many-to-many class="gov.nih.nci.cananolab.domain.particle.NanoparticleSample" column="file_pk_id" />
			</set>

		</joined-subclass>
		<joined-subclass name="gov.nih.nci.cananolab.domain.common.ProtocolFile" table="protocol_file" lazy="true">
			<key column="protocol_file_pk_id" />
			<!-- Associations mapping for the ProtocolFile class -->
			<many-to-one name="protocol" class="gov.nih.nci.cananolab.domain.common.Protocol" column="protocol_pk_id" lazy="false" />

		</joined-subclass>
	</class>
</hibernate-mapping>
