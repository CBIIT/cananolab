<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="DataSet" table="data_set" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="data_set_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the DataSet class -->
		<!-- Associations mapping for the DataSet class -->
		<many-to-one name="file" class="gov.nih.nci.cananolab.domain.common.File" column="file_pk_id" unique="true" lazy="proxy" />
	</class>
</hibernate-mapping>
