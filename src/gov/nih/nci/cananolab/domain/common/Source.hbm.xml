<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Source" table="source" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="source_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Source class -->
		<property name="address" type="string" column="address"/>
		<property name="city" type="string" column="city"/>
		<property name="country" type="string" column="country"/>
		<property name="organizationName" type="string" column="organization_name"/>
		<property name="postalCode" type="string" column="postal_code"/>
		<property name="state" type="string" column="state"/>
		<!-- Associations mapping for the Source class -->
		<set name="nanoparticleSampleCollection" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="source_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.particle.NanoparticleSample"/>
		</set>
	</class>
</hibernate-mapping>
