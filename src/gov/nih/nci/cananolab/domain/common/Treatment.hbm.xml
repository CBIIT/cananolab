<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="Treatment" table="treatment" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="treatment_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the Treatment class -->
		<property name="administrativeRoute" type="string" column="administrative_route"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="description" type="string" column="description"/>
		<property name="duration" type="string" column="duration"/>
		<property name="regimen" type="string" column="regiment"/>
		<property name="surgeryType" type="string" column="sugery_type"/>
		<!-- Associations mapping for the Treatment class -->
		<many-to-one name="animalModel" class="gov.nih.nci.cananolab.domain.common.AnimalModel" column="animal_model_pk_id" lazy="proxy" />
		<set name="treatmentGroupCollection" lazy="true">
			<cache usage="read-write" />
			<key column="treatment_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.TreatmentGroup"/>
		</set>
	</class>
</hibernate-mapping>
