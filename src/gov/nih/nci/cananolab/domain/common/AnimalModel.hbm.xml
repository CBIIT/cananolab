<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="AnimalModel" table="animal_model" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="animal_model_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the AnimalModel class -->
		<property name="age" type="double" column="age"/>
		<property name="ageUnit" type="string" column="age_unit"/>
		<property name="caModID" type="long" column="camod_id"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="description" type="string" column="description"/>
		<property name="strainName" type="string" column="strain_name"/>
		<!-- Associations mapping for the AnimalModel class -->
		<set name="studyCollection" table="study_animal_model" lazy="true" inverse="false">
			<cache usage="read-write" />
			<key column="animal_model_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Study" column="study_pk_id" />
		</set>
		<set name="treatmentCollection" lazy="true">
			<cache usage="read-write" />
			<key column="animal_model_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Treatment"/>
		</set>
		<many-to-one name="organism" class="gov.nih.nci.cananolab.domain.common.Organism" column="organism_pk_id" lazy="proxy" />
		<many-to-one name="animalDiet" class="gov.nih.nci.cananolab.domain.common.AnimalDiet" column="animal_diet_pk_id" unique="true" lazy="proxy" />
		<set name="animalCollection" lazy="true">
			<cache usage="read-write" />
			<key column="animal_model_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Animal"/>
		</set>
	</class>
</hibernate-mapping>
