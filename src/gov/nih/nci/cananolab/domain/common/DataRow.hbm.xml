<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="DataRow" table="data_row" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="data_row_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the DataRow class -->
		<!-- Associations mapping for the DataRow class -->
		<set name="datumCollection" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="data_row_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Datum"/>
		</set>
	</class>
</hibernate-mapping>
