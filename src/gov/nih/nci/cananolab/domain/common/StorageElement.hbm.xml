<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="StorageElement" table="storage" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="storage_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the StorageElement class -->
		<property name="location" type="string" column="storage_location"/>
		<property name="type" type="string" column="storage_type"/>
		<!-- Associations mapping for the StorageElement class -->
	</class>
</hibernate-mapping>
