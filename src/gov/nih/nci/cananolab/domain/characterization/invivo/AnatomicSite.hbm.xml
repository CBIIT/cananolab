<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.characterization.invivo" auto-import="false">
	<class name="AnatomicSite" table="anatomic_site" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="anatomic_site_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the AnatomicSite class -->
		<property name="conceptCode" type="string" column="concept_code"/>
		<property name="name" type="string" column="name"/>
		<!-- Associations mapping for the AnatomicSite class -->
		<set name="invivoParameter" table="invivo_parameter_anatomic_site" lazy="true">
			<cache usage="read-write" />
			<key column="anatomic_site_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.characterization.invivo.InvivoParameter" column="invivo_parameter_pk_id" unique="true"/>
		</set>
	</class>
</hibernate-mapping>
