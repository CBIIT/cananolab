<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="gov.nih.nci.cananolab.domain.particle.samplecomposition.base"
	auto-import="false">
	<class name="NanoparticleEntity" table="nanoparticle_entity"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="nanoparticle_entity_pk_id">
			<generator class="hilo">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<discriminator column="discriminator" type="string" />
		<!-- Attributes mapping for the NanoparticleEntity class -->
		<property name="description" type="string" column="description" />		
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>		
		<!-- Associations mapping for the NanoparticleEntity class -->
		<many-to-one name="sampleComposition"
			class="gov.nih.nci.cananolab.domain.particle.samplecomposition.SampleComposition"
			column="composition_pk_id" lazy="proxy" cascade="save-update"/>
		<set name="fileCollection"
			table="nanoparticle_entity_file" lazy="true" inverse="false" cascade="all-delete-orphan">
			<cache usage="read-write" />
			<key column="nanoparticle_entity_pk_id" />
			<many-to-many
				class="gov.nih.nci.cananolab.domain.common.File"
				column="file_pk_id" />
		</set>
		<set name="composingElementCollection" lazy="true" cascade="all-delete-orphan" inverse="true">
			<cache usage="read-write" />
			<key column="nanoparticle_entity_pk_id" />
			<one-to-many
				class="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.ComposingElement" />
		</set>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.OtherNanoparticleEntity"
			discriminator-value="OtherNanoparticleEntity">
			<join table="other_nanoparticle_entity">
				<key column="other_nanoparticle_entity_pk_id" />
				<!-- Attributes mapping for the OtherNanoparticleEntity class -->
				<property name="type" type="string" column="type" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Polymer"
			discriminator-value="Polymer">
			<join table="polymer">
				<key column="polymer_pk_id" />
				<!-- Attributes mapping for the Polymer class -->
				<property name="crossLinkDegree" type="float"
					column="cross_link_degree" />
				<property name="crossLinked" type="boolean"
					column="is_cross_linked" />
				<property name="initiator" type="string"
					column="initiator" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Fullerene"
			discriminator-value="Fullerene">
			<join table="fullerene">
				<key column="fullerene_pk_id" />
				<!-- Attributes mapping for the Fullerene class -->
				<property name="averageDiameter" type="float"
					column="average_diameter" />
				<property name="averageDiameterUnit" type="string"
					column="average_diameter_unit" />
				<property name="numberOfCarbon" type="integer"
					column="number_of_carbon" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Dendrimer"
			discriminator-value="Dendrimer">
			<join table="dendrimer">
				<key column="dendrimer_pk_id" />
				<!-- Attributes mapping for the Dendrimer class -->
				<property name="branch" type="string" column="branch" />
				<property name="generation" type="float"
					column="generation" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.MetalParticle"
			discriminator-value="MetalParticle">

		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Biopolymer"
			discriminator-value="Biopolymer">
			<join table="biopolymer_p">
				<key column="biopolymer_pk_id" />
				<!-- Attributes mapping for the Biopolymer class -->
				<property name="name" type="string" column="name" />
				<property name="sequence" type="string"
					column="sequence" />
				<property name="type" type="string" column="type" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.QuantumDot"
			discriminator-value="QuantumDot">

		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Emulsion"
			discriminator-value="Emulsion">
			<join table="emulsion">
				<key column="emulsion_pk_id" />
				<!-- Attributes mapping for the Emulsion class -->
				<property name="polymerized" type="boolean"
					column="is_polymerized" />
				<property name="polymerName" type="string"
					column="polymer_name" />
			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.Liposome"
			discriminator-value="Liposome">
			<join table="liposome">
				<key column="liposome_pk_id" />
				<!-- Attributes mapping for the Liposome class -->
				<property name="polymerized" type="boolean"
					column="is_polymerized" />
				<property name="polymerName" type="string"
					column="polymer_name" />

			</join>
		</subclass>
		<subclass
			name="gov.nih.nci.cananolab.domain.particle.samplecomposition.base.CarbonNanotube"
			discriminator-value="CarbonNanotube">
			<join table="carbon_nanotube">
				<key column="carbon_nanotube_pk_id" />
				<!-- Attributes mapping for the CarbonNanotube class -->
				<property name="averageLength" type="float"
					column="average_length" />
				<property name="averageLengthUnit" type="string"
					column="average_length_unit" />
				<property name="chirality" type="string"
					column="chirality" />
				<property name="diameter" type="float"
					column="diameter" />
				<property name="diameterUnit" type="string"
					column="diameter_unit" />
				<property name="wallType" type="string"
					column="wall_type" />

			</join>
		</subclass>
	</class>
</hibernate-mapping>
