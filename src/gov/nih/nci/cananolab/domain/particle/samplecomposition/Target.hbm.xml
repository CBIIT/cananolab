<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.particle.samplecomposition" auto-import="false">
	<class name="Target" table="target" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="target_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<discriminator column="discriminator" type="string"/>
		<!-- Attributes mapping for the Target class -->
		<property name="description" type="string" column="description"/>
		<property name="name" type="string" column="name"/>
		<!-- Associations mapping for the Target class -->
		<subclass name="gov.nih.nci.cananolab.domain.particle.samplecomposition.Antigen" discriminator-value="Antigen">
			<!-- Attributes mapping for the Antigen class -->
			<property name="species" type="string" column="antigen_species"/>

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.particle.samplecomposition.OtherTarget" discriminator-value="OtherTarget">
			<!-- Attributes mapping for the OtherTarget class -->
			<property name="type" type="string" column="other_target_type"/>

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.particle.samplecomposition.Receptor" discriminator-value="Receptor">

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.particle.samplecomposition.Gene" discriminator-value="Gene">

		</subclass>
	</class>
</hibernate-mapping>
