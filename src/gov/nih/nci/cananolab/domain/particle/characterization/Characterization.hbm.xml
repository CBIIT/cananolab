<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.particle.characterization" auto-import="false">
	<class name="Characterization" table="characterization" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="characterization_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<discriminator column="discriminator" type="string"/>
		<!-- Attributes mapping for the Characterization class -->
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="date" type="java.util.Date" column="characterization_date"/>
		<property name="description" type="string" column="description"/>
		<property name="identificationName" type="string" column="identifier_name"/>
		<!-- Associations mapping for the Characterization class -->
		<many-to-one name="nanoparticleSample" class="gov.nih.nci.cananolab.domain.particle.NanoparticleSample" column="particle_sample_pk_id" lazy="proxy" cascade="save-update"/>
		<set name="experimentConfigCollection" inverse="false" lazy="true" cascade="save-update, delete">
			<cache usage="read-write" />
			<key column="characterization_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.ExperimentConfig"/>
		</set>
		<many-to-one name="protocolFile" class="gov.nih.nci.cananolab.domain.common.ProtocolFile" column="protocol_file_pk_id" lazy="proxy"/>
		<many-to-one name="pointOfContact" class="gov.nih.nci.cananolab.domain.common.PointOfContact" column="poc_pk_id" lazy="proxy" />
		<set name="fileCollection" table="characterization_file" lazy="true">
			<cache usage="read-write" />
			<key column="characterization_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.File" column="file_pk_id" unique="true"/>
		</set>
		<set name="datumCollection" lazy="true">
			<cache usage="read-write" />
			<key column="characterization_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.particle.characterization.Datum"/>
		</set>
		<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.InvitroCharacterization" discriminator-value="InvitroCharacterization">
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Toxicity" discriminator-value="Toxicity">
				<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.OxidativeStress" discriminator-value="OxidativeStress">

				</subclass>
				<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Immunotoxicity" discriminator-value="Immunotoxicity">
					<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.BloodContact" discriminator-value="BloodContact">
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.PlateletAggregation" discriminator-value="PlateletAggregation">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Coagulation" discriminator-value="Coagulation">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Hemolysis" discriminator-value="Hemolysis">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.PlasmaProteinBinding" discriminator-value="PlasmaProteinBinding">

						</subclass>

					</subclass>
					<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.ImmuneCellFunction" discriminator-value="ImmuneCellFunction">
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.ComplementActivation" discriminator-value="ComplementActivation">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.NKCellCytotoxicActivity" discriminator-value="NKCellCytotoxicActivity">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Chemotaxis" discriminator-value="Chemotaxis">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.CFU_GM" discriminator-value="CFU_GM">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.OxidativeBurst" discriminator-value="OxidativeBurst">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.CytokineInduction" discriminator-value="CytokineInduction">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.LeukocyteProliferation" discriminator-value="LeukocyteProliferation">

						</subclass>
						<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Phagocytosis" discriminator-value="Phagocytosis">

						</subclass>

					</subclass>

				</subclass>
				<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.EnzymeInduction" discriminator-value="EnzymeInduction">

				</subclass>
				<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Cytotoxicity" discriminator-value="Cytotoxcity">
					<!-- Attributes mapping for the Cytotoxicity class -->
					<property name="cellDeathMethod" type="string" column="cytotoxicity_cell_death_method"/>
					<property name="cellLine" type="string" column="cytotoxicity_cell_line"/>
					<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.Caspase3Activation" discriminator-value="Caspase3Activation">

					</subclass>
					<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.invitro.CellViability" discriminator-value="CellViability">

					</subclass>

				</subclass>

			</subclass>

		</subclass>
		<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.PhysicalCharacterization" discriminator-value="PhysicalCharacterization">
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.Purity" discriminator-value="Purity">

			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.Size" discriminator-value="Size">

			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.Shape" discriminator-value="Shape">
				<join table="shape">
					<key column="shape_pk_id" />
				<!-- Attributes mapping for the Shape class -->
				<property name="aspectRatio" type="float" column="aspect_ratio"/>
				<property name="maxDimension" type="float" column="max_dimension"/>
				<property name="maxDimensionUnit" type="string" column="max_dimension_unit"/>
				<property name="minDimension" type="float" column="min_dimension"/>
				<property name="minDimensionUnit" type="string" column="min_dimension_unit"/>
				<property name="type" type="string" column="type"/>

				</join>
			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.PhysicalState" discriminator-value="PhysicalState">
				<join table="physical_state">
					<key column="physical_state_pk_id" />
				<!-- Attributes mapping for the PhysicalState class -->
				<property name="type" type="string" column="type"/>

				</join>
			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.Solubility" discriminator-value="Solubility">
				<join table="solubility">
					<key column="solubility_pk_id" />
				<!-- Attributes mapping for the Solubility class -->
				<property name="criticalConcentration" type="float" column="critical_concentration"/>
				<property name="criticalConcentrationUnit" type="string" column="critical_concentration_unit"/>
				<property name="isSoluble" type="boolean" column="is_soluble"/>
				<property name="solvent" type="string" column="solvent"/>

				</join>
			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.MolecularWeight" discriminator-value="MolecularWeight">

			</subclass>
			<subclass name="gov.nih.nci.cananolab.domain.particle.characterization.physical.Surface" discriminator-value="Surface">
				<!-- Attributes mapping for the Surface class -->
				<property name="isHydrophobic" type="boolean" column="surface_is_hydrophobic"/>
				<!-- Associations mapping for the Surface class -->
				<set name="surfaceChemistryCollection" lazy="false" cascade="all-delete-orphan" >
					<cache usage="read-write" />
					<key column="surface_pk_id" />
					<one-to-many class="gov.nih.nci.cananolab.domain.particle.characterization.physical.SurfaceChemistry"/>
				</set>

			</subclass>

		</subclass>
	</class>
</hibernate-mapping>
