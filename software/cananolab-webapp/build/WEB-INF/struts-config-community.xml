<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="collaborationGroupForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
			<form-property name="group"
				type="gov.nih.nci.cananolab.dto.common.CollaborationGroupBean" />
		</form-bean>
	</form-beans>
	<global-exceptions>
	</global-exceptions>
	<global-forwards>
	</global-forwards>
	<action-mappings>
		<action path="/manageCommunity"
			type="gov.nih.nci.cananolab.ui.community.ManageCommunityAction"
			parameter="community.manageCommunity" scope="request" />
		<action name="collaborationGroupForm" parameter="dispatch"
			path="/collaborationGroup" type="gov.nih.nci.cananolab.ui.community.CollaborationGroupAction"
			input="input">
			<exception handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
				key="error.collobrationGroup.noaccess" path="/collaborationGroup.do?dispatch=input&amp;page=0"
				type="gov.nih.nci.cananolab.exception.NoAccessException" />
			<exception handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
				key="error.collobrationGroup.duplicate" path="/collaborationGroup.do?dispatch=input&amp;page=0"
				type="gov.nih.nci.cananolab.exception.DuplicateEntriesException" />
			<forward name="setup" path="community.manageCollaborationGroup" />
			<forward name="input"
				path="/collaborationGroup.do?dispatch=input&amp;page=0" />
		</action>
	</action-mappings>
</struts-config>
