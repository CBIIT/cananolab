<hibernate-mapping auto-import="false" package="gov.nih.nci.cananolab.domain.common">
 <class lazy="true" name="Datum" polymorphism="explicit" table="datum">
  <cache include="all" usage="read-write"/>
  <id column="datum_pk_id" name="id" type="long">
   <generator class="hilo">
    <param name="table">hibernate_unique_key</param>
    <param name="column">next_hi</param>
   </generator>
  </id>
  <!-- Attributes mapping for the Datum class -->
  <property column="created_by" generated="never" lazy="false"
   name="createdBy" type="string"/>
  <property column="created_date" generated="never" lazy="false"
   name="createdDate" type="java.util.Date"/>
  <property column="name" generated="never" lazy="false" name="name" type="string"/>
  <property column="value" generated="never" lazy="false" name="value" type="float"/>
  <property column="value_type" generated="never" lazy="false"
   name="valueType" type="string"/>
  <property column="value_unit" generated="never" lazy="false"
   name="valueUnit" type="string"/>
  <property column="numberMod" generated="never" lazy="false" name="operand" type="string"/>
  <!-- Associations mapping for the Datum class -->
  <set cascade="save-update" lazy="true" name="conditionCollection"
   sort="unsorted" table="datum_condition">
   <cache include="all" usage="read-write"/>
   <key column="datum_pk_id"/>
   <many-to-many class="gov.nih.nci.cananolab.domain.common.Condition"
    column="condition_pk_id" unique="false"/>
  </set>
  <many-to-one class="gov.nih.nci.cananolab.domain.common.Finding"
   column="finding_pk_id" lazy="proxy" name="finding"/>
  <many-to-one class="gov.nih.nci.cananolab.domain.common.File"
   column="file_pk_id" lazy="proxy" name="file"/>
 </class>
</hibernate-mapping>
