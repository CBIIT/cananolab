<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="searchNanoparticleForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleSource" type="java.lang.String" />
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionTypes" type="java.lang.String[]" />
			<form-property name="characterizations" type="java.lang.String[]" />
			<form-property name="keywords" type="java.lang.String" />
			<form-property name="keywordType" type="java.lang.String" initial="nanoparticle" />
		</form-bean>
		<form-bean name="searchReportForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionTypes" type="java.lang.String[]" />
			<form-property name="reportTitle" type="java.lang.String" />
			<form-property name="reportType" type="java.lang.String" />
		</form-bean>
		<form-bean name="searchProtocolForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="fileTitle" type="java.lang.String" />
			<form-property name="protocolType" type="java.lang.String" />
			<form-property name="protocolName" type="java.lang.String" />
		</form-bean>
		<form-bean name="searchNanoparticleAdvancedForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionTypes" type="java.lang.String[]" />
			<form-property name="size1" type="gov.nih.nci.calab.dto.common.SearchableBean" />
			<form-property name="size2" type="gov.nih.nci.calab.dto.common.SearchableBean" />
			<form-property name="molecularWeight" type="gov.nih.nci.calab.dto.common.SearchableBean" />
			<form-property name="cellViability" type="gov.nih.nci.calab.dto.common.SearchableBean" />
		</form-bean>
		<form-bean name="remoteSearchNanoparticleForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionTypes" type="java.lang.String[]" />
			<form-property name="characterizations" type="java.lang.String[]" />
			<form-property name="gridNodes" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="remoteSearchReportForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionTypes" type="java.lang.String[]" />
			<form-property name="reportTitle" type="java.lang.String" />
			<form-property name="reportType" type="java.lang.String" />
			<form-property name="gridNodes" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="remoteNanoparticleGeneralInfoForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="particleName" type="java.lang.String" />
		</form-bean>
	</form-beans>
	<global-forwards>
		<forward name="remoteComposition" path="remoteNanoparticleComposition.do" />
		<forward name="construction" path="underConstruction.do" />
		<forward name="remoteSearchMessage" path="search.remote.message" />
	</global-forwards>
	<action-mappings>
		<action input="input" name="searchNanoparticleForm" parameter="dispatch" path="/searchNanoparticle" type="gov.nih.nci.calab.ui.search.SearchNanoparticleAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchNanoparticle" path="search.searchnanoparticle" type="java.lang.Exception" />
			<forward name="input" path="search.searchnanoparticle" />
			<forward name="success" path="search.searchnanoparticle.result" />
		</action>
		<action input="input" name="searchReportForm" parameter="dispatch" path="/searchReport" type="gov.nih.nci.calab.ui.search.SearchReportAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" key="error.download.file" path="home.error" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchReport" path="search.searchreport" type="java.lang.Exception" />
			<forward name="input" path="search.searchreport" />
			<forward name="success" path="search.searchreport.result" />
		</action>
		<action input="input" name="searchProtocolForm" parameter="dispatch" path="/searchProtocol" type="gov.nih.nci.calab.ui.search.SearchProtocolAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" key="error.download.file" path="home.error" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchProtocol" path="search.searchprotocol" type="java.lang.Exception" />
			<forward name="input" path="search.searchprotocol" />
			<forward name="success" path="search.searchprotocol.result" />
		</action>
		<action path="/protocolResults" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="search.searchprotocol.result" scope="request" />
		<action input="input" name="searchNanoparticleAdvancedForm" parameter="dispatch" path="/searchNanoparticleAdvanced" type="gov.nih.nci.calab.ui.search.SearchNanoparticleAdvancedAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchNanoparticleAdvanced" path="search.searchnanoparticle.advanced" type="java.lang.Exception" />
			<forward name="input" path="search.searchnanoparticle.advanced" />
			<forward name="success" path="search.searchnanoparticle.advanced.result" />
		</action>
		<action path="/reportResults" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="search.searchreport.result" scope="request" />
		<action path="/underConstruction" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="search.remote.under.construction" scope="request" />
		<action input="input" name="remoteSearchNanoparticleForm" parameter="dispatch" path="/remoteSearchNanoparticle" type="gov.nih.nci.calab.ui.search.RemoteSearchNanoparticleAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchNanoparticle" path="search.remote.searchnanoparticle" type="java.lang.Exception" />
			<forward name="input" path="search.remote.searchnanoparticle" />
			<forward name="success" path="search.remote.searchnanoparticle.result" />
		</action>
		<action input="input" name="remoteSearchReportForm" parameter="dispatch" path="/remoteSearchReport" type="gov.nih.nci.calab.ui.search.RemoteSearchReportAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" key="error.download.file" path="home.error" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchReport" path="search.remote.searchreport" type="java.lang.Exception" />
			<forward name="input" path="search.remote.searchreport" />
			<forward name="success" path="search.remote.searchreport.result" />
		</action>
		<action name="remoteNanoparticleGeneralInfoForm" parameter="dispatch" path="/remoteNanoparticleGeneralInfo" type="gov.nih.nci.calab.ui.search.RemoteNanoparticleGeneralInfoAction" input="success" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.nanoparticle.generalInfo" path="search.remote.nanoparticle.generalinfo" type="java.lang.Exception" />
			<forward name="success" path="search.remote.nanoparticle.generalinfo" />
			<forward name="viewDisclaimer" path="submit.viewParticleDisclaimer" />
		</action>
		<action name="nanoparticleCompositionForm" parameter="dispatch" path="/remoteNanoparticleComposition" type="gov.nih.nci.calab.ui.search.RemoteNanoparticleCompositionAction" input="success">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.particle.composition.view" path="search.remote.particle.composition" type="java.lang.Exception" />
			<forward name="success" path="search.remote.particle.composition" />
			<forward name="dendrimer" path="bodyDendrimerInfo.jsp" />
			<forward name="liposome" path="bodyLiposomeInfo.jsp" />
			<forward name="polymer" path="bodyPolymerInfo.jsp" />
			<forward name="carbon nanotube" path="bodyCarbonNanotubeInfo.jsp" />
			<forward name="fullerene" path="bodyFullereneInfo.jsp" />
			<forward name="quantum dot" path="bodyQuantumdotInfo.jsp" />
			<forward name="emulsion" path="bodyEmulsionInfo.jsp" />
			<forward name="metal particle" path="bodyMetalParticleInfo.jsp" />
			<forward name="complex particle" path="bodyComplexParticleInfo.jsp" />
		</action>
	</action-mappings>
</struts-config>
