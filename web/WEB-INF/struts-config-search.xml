<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="searchWorkflowForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="fileName" type="java.lang.String" />
			<form-property name="assayRunDateBegin" type="java.lang.String" />
			<form-property name="assayRunDateEnd" type="java.lang.String" />
			<form-property name="assayName" type="java.lang.String" />
			<form-property name="fileSubmissionDateEnd" type="java.lang.String" />
			<form-property name="isFileIn" type="java.lang.String" initial="in" />
			<form-property name="isFileOut" type="java.lang.String" initial="out" />
			<form-property name="aliquotName" type="java.lang.String" />
			<form-property name="excludeMaskedAliquots" type="java.lang.String" />
			<form-property name="assayType" type="java.lang.String" />
			<form-property name="fileSubmissionDateBegin" type="java.lang.String" />
			<form-property name="fileSubmitter" type="java.lang.String" />
			<form-property name="excludeMaskedFiles" type="java.lang.String" />
			<form-property name="criteriaJoin" type="java.lang.String" initial="and" />
		</form-bean>
		<form-bean name="searchSampleForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="sampleName" type="java.lang.String" />
			<form-property name="sourceSampleId" type="java.lang.String" />
			<form-property name="sampleSource" type="java.lang.String" />
			<form-property name="dateAccessionedBegin" type="java.lang.String" />
			<form-property name="sampleType" type="java.lang.String" />
			<form-property name="dateAccessionedEnd" type="java.lang.String" />
			<form-property name="sampleSubmitter" type="java.lang.String" />
			<form-property name="storageLocation" type="gov.nih.nci.calab.dto.inventory.StorageLocation" />
		</form-bean>
		<form-bean name="searchAliquotForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="aliquotName" type="java.lang.String" />
			<form-property name="containerId" type="java.lang.String" />
			<form-property name="sourceSampleId" type="java.lang.String" />
			<form-property name="sampleSource" type="java.lang.String" />
			<form-property name="dateAccessionedBegin" type="java.lang.String" />
			<form-property name="sampleType" type="java.lang.String" />
			<form-property name="dateAccessionedEnd" type="java.lang.String" />
			<form-property name="sampleSubmitter" type="java.lang.String" />
			<form-property name="storageLocation" type="gov.nih.nci.calab.dto.inventory.StorageLocation" />
		</form-bean>
		<form-bean name="viewSampleDetailForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="sampleId" type="java.lang.String" />
			<form-property name="containerId" type="java.lang.String" />
		</form-bean>
		<form-bean name="viewAliquotDetailForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="sampleId" type="java.lang.String" />
			<form-property name="aliquotId" type="java.lang.String" />
		</form-bean>
		<form-bean name="searchNanoparticleForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleSource" type="java.lang.String" />
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="functionType" type="java.lang.String" />
			<form-property name="characterizationType" type="java.lang.String" />
			<form-property name="keywords" type="java.lang.String" />
		</form-bean>
	</form-beans>
	<action-mappings>
		<action input="input" name="searchWorkflowForm" parameter="dispatch" path="/searchWorkflow" type="gov.nih.nci.calab.ui.search.SearchWorkflowAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchWorkflow" path="search.searchworkflow" type="java.lang.Exception" />
			<forward name="input" path="search.searchworkflow" />
			<forward name="success" path="search.searchworkflowresult" />
		</action>
		<action input="input" name="searchSampleForm" parameter="dispatch" path="/searchSample" type="gov.nih.nci.calab.ui.search.SearchSampleAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchSample" path="search.searchsample" type="java.lang.Exception" />
			<forward name="input" path="search.searchsample" />
			<forward name="success" path="search.searchsampleresult" />
		</action>
		<action input="input" name="searchAliquotForm" parameter="dispatch" path="/searchAliquot" type="gov.nih.nci.calab.ui.search.SearchAliquotAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchAliquot" path="search.searchaliquot" type="java.lang.Exception" />
			<forward name="input" path="search.searchaliquot" />
			<forward name="success" path="search.searchaliquotresult" />
		</action>
		<action path="/sampleResultForward" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="search.searchsampleresult" scope="request" />
		<action input="input" name="viewSampleDetailForm" path="/viewSampleDetail" scope="request" type="gov.nih.nci.calab.ui.search.ViewSampleDetailAction">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.viewSampleDetails.noSamples" path="search.searchsampleresult" type="java.lang.Exception" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.viewSampleDetails" path="search.searchsampleresult" type="java.lang.Exception" />
			<forward name="input" path="search.searchsampleresult" />
			<forward name="success" path="search.viewsampledetails" />
		</action>
		<action input="input" name="viewAliquotDetailForm" path="/viewAliquotDetail" scope="request" type="gov.nih.nci.calab.ui.search.ViewAliquotDetailAction">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.viewSampleDetails.noSamples" path="search.searchsampleresult" type="java.lang.Exception" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.viewSampleDetails" path="search.searchsampleresult" type="java.lang.Exception" />
			<forward name="input" path="search.searchaliquotresult" />
			<forward name="success" path="search.viewaliquotdetails" />
		</action>
		<action name="fileDownloadForm" parameter="method" path="/downloadSearchedFile" scope="request" type="gov.nih.nci.calab.ui.workflow.FileDownloadAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.downloadSearchedFile.nofile" path="search.message" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.downloadSearchedFile" path="search.message" type="java.lang.Exception" />
			<forward name="input" path="search.searchworkflowresult" />
		</action>
		<action input="input" name="searchNanoparticleForm" parameter="dispatch" path="/searchNanoparticle" type="gov.nih.nci.calab.ui.search.SearchNanoparticleAction" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="search" key="error.searchNanoparticle" path="search.searchnanoparticle" type="java.lang.Exception" />
			<forward name="input" path="search.searchnanoparticle" />
			<forward name="success" path="search.searchnanoparticle.result" />
		</action>
	</action-mappings>

</struts-config>
