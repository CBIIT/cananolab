		<action path="/manageNanoparticle" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="submit.managenanoparticle" scope="request" />
		<action name="nanoparticleGeneralInfoForm" parameter="dispatch" path="/nanoparticleGeneralInfo" type="gov.nih.nci.calab.ui.submit.NanoparticleGeneralInfoAction" input="input" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.nanoparticle.generalInfo" path="submit.createnanoparticle" type="java.lang.Exception" />
			<forward name="success" path="submit.message" />
			<forward name="view" path="submit.generalinfo.view" />
			<forward name="viewDisclaimer" path="submit.viewParticleDisclaimer" />
			<forward name="update" path="submit.generalinfo.update" />
			<forward name="input" path="submit.createnanoparticle" />
		</action>
		<action name="submitProtocolForm" parameter="dispatch" path="/submitProtocol" type="gov.nih.nci.calab.ui.submit.SubmitProtocolAction" input="input" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.submitProtocol" path="submit.submitprotocol" type="java.lang.Exception" />
			<forward name="success" path="submit.protocol.message" />
			<forward name="input" path="/submitProtocol.do?dispatch=reSetup&amp;page=0" />
			<forward name="setup" path="submit.submitprotocol" />
			<forward name="resetProtocolPage" path="submit.submitprotocol" />
		</action>
		<action name="updateProtocolForm" parameter="dispatch" path="/updateProtocol" type="gov.nih.nci.calab.ui.submit.SubmitProtocolAction" input="input" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.updateProtocol" path="submit.updateprotocol" type="java.lang.Exception" />
			<forward name="success" path="submit.protocol.message" />
			<forward name="input" path="/updateProtocol.do?dispatch=setupUpdate&amp;page=0" />
			<forward name="failure" path="/updateProtocol.do?dispatch=setupUpdate" />
			<forward name="setup" path="submit.updateprotocol" />
		</action>
