<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="nanoparticleGeneralInfoForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="particleName" type="java.lang.String" />
			<form-property name="keywords" type="java.lang.String" />
			<form-property name="visibilities" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="submitReportForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="particleNames" type="java.lang.String[]" />
			<form-property name="reportFile" type="org.apache.struts.upload.FormFile" />
			<form-property name="visibilities" type="java.lang.String[]" />
			<form-property name="title" type="java.lang.String" />
			<form-property name="description" type="java.lang.String" />
		</form-bean>
		<form-bean name="addAssayResultForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="particleName" type="java.lang.String" />
			<form-property name="fileId" type="java.lang.String" />
			<form-property name="title" type="java.lang.String" />
			<form-property name="description" type="java.lang.String" />
			<form-property name="comments" type="java.lang.String" />
			<form-property name="keywords" type="java.lang.String" />
		</form-bean>
		<form-bean name="nanoparticleCompositionForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer" initial="0" />
			<form-property name="particleName" type="java.lang.String" />
			<form-property name="particleType" type="java.lang.String" />
			<form-property name="dendrimer" type="gov.nih.nci.calab.dto.characterization.composition.DendrimerBean" />
			<form-property name="liposome" type="gov.nih.nci.calab.dto.characterization.composition.LiposomeBean" />
			<form-property name="buckeyball" type="gov.nih.nci.calab.dto.characterization.composition.BuckeyballBean" />
			<form-property name="fullerene" type="gov.nih.nci.calab.dto.characterization.composition.FullereneBean" />
			<form-property name="polymer" type="gov.nih.nci.calab.dto.characterization.composition.PolymerBean" />
			<form-property name="quantumDot" type="gov.nih.nci.calab.dto.characterization.composition.QuantumDotBean" />
			<form-property name="metalParticle" type="gov.nih.nci.calab.dto.characterization.composition.MetalParticleBean" />
			<form-property name="particlePage" type="java.lang.String" />
		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/manageNanoparticle" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="submit.managenanoparticle" scope="request" />
		<action name="nanoparticleGeneralInfoForm" parameter="dispatch" path="/nanoparticleGeneralInfo" type="gov.nih.nci.calab.ui.submit.NanoparticleGeneralInfoAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.nanoparticle.generalInfo" path="submit.createnanoparticle" type="java.lang.Exception" />
			<forward name="success" path="submit.message" />
			<forward name="view" path="submit.generalinfo.view"/>
			<forward name="update" path="submit.generalinfo.update"/>
			<forward name="input" path="submit.createnanoparticle" />
		</action>
		<action name="submitReportForm" parameter="dispatch" path="/submitReport" type="gov.nih.nci.calab.ui.submit.SubmitReportAction" input="input" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.submitReport" path="submit.submitreport" type="java.lang.Exception" />
			<forward name="success" path="submit.message" />
			<forward name="input" path="submit.submitreport" />
		</action>
		<action name="addAssayResultForm" parameter="dispatch" path="/addAssayResult" type="gov.nih.nci.calab.ui.submit.AddAssayResultAction" input="input" scope="request">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.addAssayResult" path="submit.addassayresult" type="java.lang.Exception" />
			<forward name="success" path="submit.message" />
			<forward name="input" path="submit.addassayresult" />
		</action>
		<action name="nanoparticleCompositionForm" parameter="dispatch" path="/nanoparticleComposition" type="gov.nih.nci.calab.ui.submit.NanoparticleCompositionAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="submit" key="error.addParticleComposition" path="submit.addparticle.composition" type="java.lang.Exception" />
			<forward name="success" path="submit.message" />
			<forward name="input" path="submit.addparticle.composition" />
			<forward name="dendrimer" path="bodyDendrimerInfo.jsp" />
			<forward name="liposome" path="bodyLiposomeInfo.jsp" />
			<forward name="polymer" path="bodyPolymerInfo.jsp" />
			<forward name="buckeyball" path="bodyBuckeyballInfo.jsp" />
			<forward name="fullerene" path="bodyFullereneInfo.jsp" />
			<forward name="quantum dot" path="bodyQuantumdotInfo.jsp" />
			<forward name="metal particle" path="bodyMetalParticleInfo.jsp" />
		</action>
		<action path="/submitAction" type="gov.nih.nci.calab.ui.core.BaseForwardAction" parameter="submit.action.message" scope="request" />
	</action-mappings>
</struts-config>
