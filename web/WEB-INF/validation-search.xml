<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="searchWorkflowForm">
			<field property="assayRunDateBegin" depends="date">
				<arg key="searchWorkflowForm.assayRunDateBegin" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
			<field property="assayRunDateEnd" depends="date">
				<arg key="searchWorkflowForm.assayRunDateEnd" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
			<field property="fileSubmissionDateBegin" depends="date">
				<arg key="searchWorkflowForm.fileSubmissionDateBegin" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
			<field property="fileSubmissionDateEnd" depends="date">
				<arg key="searchWorkflowForm.fileSubmissionDateEnd" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
		</form>
		<form name="searchSampleForm">
			<field property="dateAccessionedBegin" depends="date">
				<arg key="searchSampleForm.dateAccessionedBegin" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
			<field property="dateAccessionedEnd" depends="date">
				<arg key="searchSampleForm.dateAccessionedEnd" bundle="search" position="0" />
				<var>
					<var-name>datePattern</var-name>
					<var-value>MM/dd/yy</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>
