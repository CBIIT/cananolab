<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="submitPointOfContactForm">
			<field property="poc.organization.name" page="2"
				depends="required">
				<arg position="0" key="submitPointOfContactForm.organizationName"
					bundle="pointOfContact" />
			</field>
			<field property="organization.name" page="2" depends="required"
				indexedListProperty="otherPoc.otherPointOfContacts">
				<arg position="0" key="submitPointOfContactForm.organizationName"
					bundle="pointOfContact" />
			</field>
		</form>		
	</formset>
</form-validation>
