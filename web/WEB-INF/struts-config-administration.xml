<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="createSampleForm" type="org.apache.struts.validator.DynaValidatorActionForm">
			<form-property name="sampleNamePrefix" type="java.lang.String" />
			<form-property name="sampleType" type="java.lang.String" />
			<form-property name="otherSampleType" type="java.lang.String" />
			<form-property name="sampleSOP" type="java.lang.String" />
			<form-property name="sampleDescription" type="java.lang.String" />
			<form-property name="sampleSource" type="java.lang.String" />
			<form-property name="sourceSampleId" type="java.lang.String" />
			<form-property name="dateReceived" type="java.lang.String" />
			<form-property name="lotId" type="java.lang.String" initial="N/A" />
			<form-property name="lotDescription" type="java.lang.String" />
			<form-property name="solubility" type="java.lang.String" />
			<form-property name="numberOfContainers" type="java.lang.String" initial="1" />
			<form-property name="generalComments" type="java.lang.String" />
			<form-property name="containers" type="gov.nih.nci.calab.dto.administration.ContainerBean[]" size="100" />
			<form-property name="configuredSampleNamePrefix" type="java.lang.String" />
		</form-bean>
		<form-bean name="createAliquotForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="sampleName" type="java.lang.String" />
			<form-property name="parentAliquotName" type="java.lang.String" />
			<form-property name="numberOfAliquots" type="java.lang.String" initial="0" />
			<form-property name="template" type="gov.nih.nci.calab.dto.administration.AliquotBean" />
			<form-property name="generalComments" type="java.lang.String" />
		</form-bean>
		<form-bean name="editAliquotForm" type="org.apache.struts.validator.DynaValidatorActionForm">
			<form-property name="rowNum" type="java.lang.String" />
			<form-property name="colNum" type="java.lang.String" />
			<form-property name="aliquot" type="gov.nih.nci.calab.dto.administration.AliquotBean" />
		</form-bean>
	</form-beans>

	<global-forwards>
		<forward name="createSample" path="/preCreateSample.do" />
		<forward name="createAliquot" path="administration.createaliquot" />
	</global-forwards>

	<action-mappings>
		<action name="createSampleForm" path="/preCreateSample" type="gov.nih.nci.calab.ui.administration.PreCreateSampleAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.preCreateSample" path="administration.message" type="java.lang.Exception" />
			<forward name="success" path="administration.createsample" />
			<forward name="input" path="administration.createsample" />
		</action>

		<action name="createSampleForm" path="/createSample" type="gov.nih.nci.calab.ui.administration.CreateSampleAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.createSample" path="administration.createsample" type="java.lang.Exception" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.createSample.duplicate" path="administration.createsample" type="gov.nih.nci.calab.exception.DuplicateEntriesException" />
			<forward name="success" path="administration.sampledetails" />
			<forward name="input" path="administration.createsample" />
		</action>
		<action name="createAliquotForm" path="/createAliquot" type="gov.nih.nci.calab.ui.administration.CreateAliquotAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.createAliquot.nomatrix" path="administration.createaliquot" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.createAliquot.duplicate" path="administration.createaliquot" type="gov.nih.nci.calab.exception.DuplicateEntriesException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.createAliquot" path="administration.createaliquot" type="java.lang.Exception" />
			<forward name="input" path="administration.createaliquot" />
			<forward name="success" path="administration.aliquotdetails" />
		</action>
		<action name="createAliquotForm" path="/preCreateAliquot" type="gov.nih.nci.calab.ui.administration.PreCreateAliquotAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.preCreateAliquot" path="administration.message" type="java.lang.Exception" />
			<forward name="input" path="administration.createaliquot" />
			<forward name="success" path="administration.createaliquot" />
		</action>
		<action name="editAliquotForm" path="/preEditAliquot" scope="request" type="gov.nih.nci.calab.ui.administration.PreEditAliquotAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.preEditAliquot.nomatrix" path="administration.createaliquot" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.preEditAliquot" path="administration.createaliquot" type="java.lang.Exception" />
			<forward name="input" path="administration.createaliquot" />
			<forward name="success" path="administration.editaliquot" />
		</action>
		<action name="editAliquotForm" path="/editAliquot" scope="request" type="gov.nih.nci.calab.ui.administration.EditAliquotAction" input="input">
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.editAliquot.nomatrix" path="administration.createaliquot" type="gov.nih.nci.calab.exception.CalabException" />
			<exception handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler" bundle="administration" key="error.editAliquot" path="administration.createaliquot" type="java.lang.Exception" />
			<forward name="input" path="administration.editaliquot" />
			<forward name="success" path="administration.createaliquot" />
		</action>
	</action-mappings>
</struts-config>
