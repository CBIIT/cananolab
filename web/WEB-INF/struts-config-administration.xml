<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="createSampleForm" type="org.apache.struts.validator.DynaValidatorActionForm">
			<form-property name="sampleId" type="java.lang.String" />
			<form-property name="sampleType" type="java.lang.String" />
			<form-property name="sampleSOP" type="java.lang.String" />
			<form-property name="sampleDescription" type="java.lang.String" />
			<form-property name="vendor" type="java.lang.String" />
			<form-property name="vendorSampleId" type="java.lang.String" />
			<form-property name="dateReceived" type="java.lang.String" />
			<form-property name="lotId" type="java.lang.String" />
			<form-property name="lotDescription" type="java.lang.String" />
			<form-property name="solubility" type="java.lang.String" />
			<form-property name="numberOfContainers" type="java.lang.String" initial="1" />
			<form-property name="generalComments" type="java.lang.String" />
			<form-property name="containers" type="gov.nih.nci.calab.dto.administration.ContainerBean[]" />
		</form-bean>
		<form-bean name="createAliquotForm" type="org.apache.struts.validator.DynaValidatorActionForm">
			<form-property name="sampleId" type="java.lang.String" />
			<form-property name="aliquotId" type="java.lang.String" />
			<form-property name="numberOfAliquots" type="java.lang.String" />
			<form-property name="template" type="gov.nih.nci.calab.dto.administration.ContainerInfoBean" />
			<form-property name="lotId" type="java.lang.String" />
		</form-bean>
	</form-beans>
	<action-mappings>
		<action name="createSampleForm" path="/preCreateSample" scope="session" type="gov.nih.nci.calab.ui.administration.PreCreateSampleAction" input="input">
			<forward name="success" path="administration.createsample" />
			<forward name="failure" path="administration.message" />
			<forward name="input" path="administration.message" />
		</action>

		<action name="createSampleForm" path="/createSample" type="gov.nih.nci.calab.ui.administration.CreateSampleAction" scope="session" input="input">
			<forward name="success" path="administration.sampledetails" />
			<forward name="failure" path="administration.createsample" />
			<forward name="input" path="administration.createsample" />
		</action>
		<action attribute="createAliquotForm" name="createAliquotForm" path="/createAliquot" scope="request" type="gov.nih.nci.calab.ui.administration.CreateAliquotAction">
			<forward name="input" path="administration.createaliquot" />
			<forward name="success" path="administration.message" />
			<forward name="failure" path="administration.createaliquot" />
		</action>
	</action-mappings>
</struts-config>
