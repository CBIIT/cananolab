<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="nanoparticleSampleForm">
			<field property="particleSampleBean.domainParticleSample.name"
				page="1" depends="required">
				<arg position="0"
					key="nanoparticleSampleForm.sampleName" bundle="particle" />
			</field>
			<field
				property="particleSampleBean.domainParticleSample.source.organizationName"
				page="1" depends="required">
				<arg position="0"
					key="nanoparticleSampleForm.sampleSource" bundle="particle" />
			</field>
		</form>
		<form name="nanoparticleGeneralInfoForm">
			<field property="particle.sampleType" page="1"
				depends="required">
				<arg position="0"
					key="nanoparticleGeneralInfoForm.particleType" bundle="particle" />
			</field>
			<field property="particle.sampleName" page="1"
				depends="required">
				<arg position="0"
					key="nanoparticleGeneralInfoForm.particleID" bundle="particle" />
			</field>
		</form>
		<form name="nanoparticleCompositionForm">
			<field property="composition.characterizationSource"
				page="2" depends="required">
				<arg position="0" key="characterizationSource"
					bundle="particle" />
			</field>
			<field property="composition.viewTitle" page="2"
				depends="required">
				<arg position="0" key="viewTitle" bundle="particle" />
			</field>
			<field property="carbonNanotube.growthDiameter" page="2"
				depends="float">
				<arg position="0"
					key="nanoparticleCompositionForm.carbonNanotube.growthDiameter"
					bundle="particle" />
			</field>
			<field property="carbonNanotube.averageLength" page="2"
				depends="float">
				<arg position="0"
					key="nanoparticleCompositionForm.carbonNanotube.averageLength"
					bundle="particle" />
			</field>
			<field property="fullerene.numberOfCarbons" page="2"
				depends="integer">
				<arg position="0"
					key="nanoparticleCompositionForm.fullerene.numberOfCarbon"
					bundle="particle" />
			</field>
			<field property="liposome.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleCompositionForm.liposome.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((liposome.polymerized=="No") or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="emulsion.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleCompositionForm.emulsion.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((emulsion.polymerized=="No") or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="polymer.crosslinkDegree" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleCompositionForm.polymer.crosslinkDegree"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((polymer.crosslinked=="No") or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="elementType" page="2" depends="validwhen"
				indexedListProperty="composition.composingElements">
				<arg position="0"
					key="nanoparticleComposition.composition.elementType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
			<field property="name" page="2" depends="validwhen"
				indexedListProperty="dendrimer.surfaceGroups">
				<arg position="0"
					key="nanoparticleComposition.dendrimer.surfaceGroup"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
		</form>
		<form name="nanoparticleFunctionForm">
			<field property="function.viewTitle" page="2"
				depends="required">
				<arg position="0" key="viewTitle" bundle="particle" />
			</field>
			<field property="type" page="2" depends="validwhen"
				indexedListProperty="function.linkages">
				<arg position="0" key="function.linkageType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((function.numberOfLinkages==0) or
						(*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="agent.type" page="2" depends="validwhen"
				indexedListProperty="function.linkages">
				<arg position="0" key="function.linkage.agentType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((function.numberOfLinkages==0) or
						(*this*!=null))
					</var-value>
				</var>
			</field>
		</form>
		<form name="loadDerivedBioAssayDataForm">
			<field property="uploadedFile" page="2"
				depends="validwhen">
				<arg position="0" key="loadDerivedBioAssayDataForm.file"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((file.uri!=null) or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="file.title" page="1" depends="required">
				<arg position="0"
					key="loadDerivedBioAssayDataForm.title" bundle="particle" />
			</field>
		</form>
		<form name="nanoparticleCharacterizationForm">
			<field property="achar.characterizationSource" page="2"
				depends="required">
				<arg position="0" key="characterizationSource"
					bundle="particle" />
			</field>
			<field property="achar.viewTitle" page="2"
				depends="required">
				<arg position="0" key="viewTitle" bundle="particle" />
			</field>
			<field
				property="achar.instrumentConfigBean.instrumentBean.type" page="2"
				depends="validwhen">
				<arg position="0" key="achar.instrument.type"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfigBean.description==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field
				property="achar.instrumentConfigBean.instrumentBean.manufacturer"
				page="2" depends="validwhen">
				<arg position="0" key="achar.instrument.manufacturer"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfigBean.instrumentBean.type==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="shape.maxDimension" page="2"
				depends="float">
				<arg position="0" key="shape.maxDimension"
					bundle="particle" />
			</field>
			<field property="shape.minDimension" page="2"
				depends="float">
				<arg position="0" key="shape.minDimension"
					bundle="particle" />
			</field>
			<field property="surface.surfaceArea" page="2"
				depends="float">
				<arg position="0" key="surface.surfaceAreaValue"
					bundle="particle" />
			</field>
			<field property="surface.zetaPotential" page="2"
				depends="float">
				<arg position="0" key="surface.zetaPotentialValue"
					bundle="particle" />
			</field>
			<field property="surface.charge" page="2" depends="float">
				<arg position="0" key="surface.chargeValue"
					bundle="particle" />
			</field>
			<field property="surface.chargeUnit" page="2"
				depends="validwhen">
				<arg position="0" key="surface.chargeUnit"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((surface.charge==null) or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="numberOfMolecules" page="2"
				depends="integer, validwhen"
				indexedListProperty="surface.surfaceChemistries">
				<arg position="0" key="surface.numberOfMolecules"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
			<field property="solubility.criticalConcentration" page="2"
				depends="float">
				<arg position="0"
					key="solubility.criticalConcentrationValue" bundle="particle" />
			</field>
			<field property="solubility.criticalConcentrationUnit"
				page="2" depends="validwhen">
				<arg position="0"
					key="solubility.criticalConcentrationUnit" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((solubility.criticalConcentration==null) or
						(*this*!=null))
					</var-value>
				</var>
			</field>
		</form>
		<form name="characterizationMultiDeletionForm">
			<field property="charIds" depends="required" page="2">
				<arg position="0" key="delete.charIds"
					bundle="particle" />
			</field>
		</form>
		<form name="searchNanoparticleSampleForm">
			<field property="text" page="1" depends="mask">
				<arg position="0" key="text" bundle="particle" />
				<msg name="mask" key="text.invalid" />
				<var>
					<var-name>mask</var-name>
					<var-value>^[a-zA-Z0-9\-\_\s\"']*$</var-value>
				</var>
			</field>
		</form>
		<form name="remoteSearchNanoparticleForm">
			<field property="particleType" page="1" depends="mask">
				<arg position="0" key="particleType" bundle="particle" />
				<msg name="mask" key="particleType.invalid" />
				<var>
					<var-name>mask</var-name>
					<var-value>^[a-zA-Z0-9\-\_\s]*$</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>
