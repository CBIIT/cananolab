<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="nanoparticleSampleForm">
			<field property="particleSampleBean.domainParticleSample.name"
				page="1" depends="required">
				<arg position="0"
					key="nanoparticleSampleForm.sampleName" bundle="particle" />
			</field>
			<field
				property="particleSampleBean.domainParticleSample.source.organizationName"
				page="1" depends="required">
				<arg position="0"
					key="nanoparticleSampleForm.sampleSource" bundle="particle" />
			</field>
		</form>
		<form name="chemicalAssociationForm">
			<field property="assoc.type" page="2"
				depends="required">
				<arg position="0" key="chemicalAssociationType" bundle="particle" />
			</field>
			<field property="entity.name" page="2"
				depends="required">
				<arg position="0" key="chemicalAssociationBondType" bundle="particle" />
			</field>
		</form>
		<form name="nanoparticleEntityForm">
			<field property="entity.type" page="2"
				depends="required">
				<arg position="0" key="nanoparticleEntityType" bundle="particle" />
			</field>
			<field property="entity.carbonNanotube.diameter" page="2"
				depends="float">
				<arg position="0"
					key="nanoparticleEntityForm.carbonNanotube.diameter"
					bundle="particle" />
			</field>
			<field property="entity.carbonNanotube.averageLength" page="2"
				depends="float">
				<arg position="0"
					key="nanoparticleEntityForm.carbonNanotube.averageLength"
					bundle="particle" />
			</field>
			<field property="entity.fullerene.numberOfCarbon" page="2"
				depends="integer">
				<arg position="0"
					key="nanoparticleEntityForm.fullerene.numberOfCarbon"
					bundle="particle" />
			</field>
			<field property="entity.biopolymer.name" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.biopolymer.name"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='biopolymer') or
						 (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="entity.biopolymer.type" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.biopolymer.type"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='biopolymer') or
						 (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="entity.liposome.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.liposome.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='liposome') or
						 ((entity.liposome.polymerized=="No") or
						 (*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="entity.emulsion.polymerName" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.emulsion.polymerName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='emulsion') or
						((entity.emulsion.polymerized=="No") or (*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="entity.polymer.crossLinkDegree" page="2"
				depends="validwhen">
				<arg position="0"
					key="nanoparticleEntityForm.polymer.crossLinkDegree"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((entity.type!='polymer') or
						((entity.polymer.crossLinkDegree=="No") or (*this*!=null)))
					</var-value>
				</var>
			</field>
			<field property="domainComposingElement.type" page="2" depends="validwhen"
				indexedListProperty="entity.composingElements">
				<arg position="0"
					key="nanoparticleEntityForm.elementType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
			<field property="domainComposingElement.name" page="2" depends="validwhen"
				indexedListProperty="entity.composingElements">
				<arg position="0"
					key="nanoparticleEntityForm.elementName"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
		</form>
		<form name="functionalizingEntityForm">
			<field property="entity.type" page="2"
				depends="required">
				<arg position="0" key="functionalizingEntityType" bundle="particle" />
			</field>
			<field property="entity.name" page="2"
				depends="required">
				<arg position="0" key="functionalizingEntityName" bundle="particle" />
			</field>
			<field property="entity.value" page="2"
				depends="float">
				<arg position="0"
					key="functionalizingEntityForm.value"
					bundle="particle" />
			</field>
			<field property="type" page="2" depends="validwhen"
				indexedListProperty="entity.functions">
				<arg position="0"
					key="functionalizingEntity.functionType"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
		</form>
		<form name="loadDerivedBioAssayDataForm">
			<field property="uploadedFile" page="2"
				depends="validwhen">
				<arg position="0" key="loadDerivedBioAssayDataForm.file"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((file.uri!=null) or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="file.title" page="1" depends="required">
				<arg position="0"
					key="loadDerivedBioAssayDataForm.title" bundle="particle" />
			</field>
		</form>
		<form name="physicalCharacterizationForm">
			<field property="achar.characterizationSource" page="2"
				depends="required">
				<arg position="0" key="characterizationSource"
					bundle="particle" />
			</field>
			<field property="achar.viewTitle" page="2"
				depends="required">
				<arg position="0" key="viewTitle" bundle="particle" />
			</field>
			<field
				property="achar.instrumentConfig.instrument.type" page="2"
				depends="validwhen">
				<arg position="0" key="achar.instrument.type"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfig.description==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field
				property="achar.instrumentConfig.instrument.manufacturer"
				page="2" depends="validwhen">
				<arg position="0" key="achar.instrument.manufacturer"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfig.instrument.type==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="achar.shape.maxDimension" page="2"
				depends="float">
				<arg position="0" key="shape.maxDimension"
					bundle="particle" />
			</field>
			<field property="achar.shape.minDimension" page="2"
				depends="float">
				<arg position="0" key="shape.minDimension"
					bundle="particle" />
			</field>
			<field property="achar.surfaceBean.domainSurface.surfaceArea" page="2"
				depends="float">
				<arg position="0" key="surface.surfaceAreaValue"
					bundle="particle" />
			</field>
			<field property="achar.surfaceBean.domainSurface.zetaPotential" page="2"
				depends="float">
				<arg position="0" key="surface.zetaPotentialValue"
					bundle="particle" />
			</field>
			<field property="achar.surfaceBean.domainSurface.charge" page="2" depends="float">
				<arg position="0" key="surface.chargeValue"
					bundle="particle" />
			</field>
			<field property="achar.surfaceBean.domainSurface.chargeUnit" page="2"
				depends="validwhen">
				<arg position="0" key="surface.chargeUnit"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.surfaceBean.domainSurface.charge==null) or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field property="numberOfMolecules" page="2"
				depends="integer, validwhen"
				indexedListProperty="achar.surfaceBean.domainSurface.surfaceChemistryCollection">
				<arg position="0" key="surface.numberOfMolecules"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>(*this*!=null)</var-value>
				</var>
			</field>
			<field property="achar.solubility.criticalConcentration" page="2"
				depends="float">
				<arg position="0"
					key="solubility.criticalConcentrationValue" bundle="particle" />
			</field>
			<field property="achar.solubility.criticalConcentrationUnit"
				page="2" depends="validwhen">
				<arg position="0"
					key="solubility.criticalConcentrationUnit" bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.solubility.criticalConcentration==null) or
						(*this*!=null))
					</var-value>
				</var>
			</field>
		</form>
		<form name="invitroCharacterizationForm">
			<field property="achar.characterizationSource" page="2"
				depends="required">
				<arg position="0" key="characterizationSource"
					bundle="particle" />
			</field>
			<field property="achar.viewTitle" page="2"
				depends="required">
				<arg position="0" key="viewTitle" bundle="particle" />
			</field>
			<field
				property="achar.instrumentConfig.instrument.type" page="2"
				depends="validwhen">
				<arg position="0" key="achar.instrument.type"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfig.description==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
			<field
				property="achar.instrumentConfig.instrument.manufacturer"
				page="2" depends="validwhen">
				<arg position="0" key="achar.instrument.manufacturer"
					bundle="particle" />
				<var>
					<var-name>test</var-name>
					<var-value>
						((achar.instrumentConfig.instrument.type==null)
						or (*this*!=null))
					</var-value>
				</var>
			</field>
		</form>
		<form name="characterizationMultiDeletionForm">
			<field property="charIds" depends="required" page="2">
				<arg position="0" key="delete.charIds"
					bundle="particle" />
			</field>
		</form>
		<form name="searchNanoparticleSampleForm">
			<field property="text" page="1" depends="mask">
				<arg position="0" key="text" bundle="particle" />
				<msg name="mask" key="text.invalid" />
				<var>
					<var-name>mask</var-name>
					<var-value>^[a-zA-Z0-9\-\_\s\"']*$</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>
