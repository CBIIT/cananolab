<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="submitProtocolForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
			<form-property name="protocolName" type="java.lang.String" />
			<form-property name="protocolType" type="java.lang.String" />
			<form-property name="uploadedFile"
				type="org.apache.struts.upload.FormFile" />
			<form-property name="file"
				type="gov.nih.nci.calab.dto.common.ProtocolFileBean" />
		</form-bean>
		<form-bean name="updateProtocolForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
			<form-property name="protocolName" type="java.lang.String" />
			<form-property name="protocolType" type="java.lang.String" />
			<form-property name="uploadedFile"
				type="org.apache.struts.upload.FormFile" />
			<form-property name="file"
				type="gov.nih.nci.calab.dto.common.ProtocolFileBean" />
		</form-bean>
		<form-bean name="searchProtocolForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="fileTitle" type="java.lang.String" />
			<form-property name="protocolType" type="java.lang.String" />
			<form-property name="protocolName" type="java.lang.String" />
		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/manageProtocol"
			type="gov.nih.nci.calab.ui.core.BaseForwardAction"
			parameter="protocol.manageProtocol" scope="request" />
		<action name="submitProtocolForm" parameter="dispatch"
			path="/submitProtocol"
			type="gov.nih.nci.calab.ui.protocol.SubmitProtocolAction"
			input="input" scope="request">
			<exception
				handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler"
				bundle="protocol" key="error.submitProtocol"
				path="protocol.submitProtocol" type="java.lang.Exception" />
			<forward name="success" path="protocol.message" />
			<forward name="input"
				path="/submitProtocol.do?dispatch=reSetup&amp;page=0" />
			<forward name="setup" path="protocol.submitProtocol" />
			<forward name="resetProtocolPage"
				path="protocol.submitProtocol" />
		</action>
		<action name="updateProtocolForm" parameter="dispatch"
			path="/updateProtocol"
			type="gov.nih.nci.calab.ui.protocol.SubmitProtocolAction"
			input="input" scope="request">
			<exception
				handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler"
				bundle="protocol" key="error.updateProtocol"
				path="protocol.updateProtocol" type="java.lang.Exception" />
			<forward name="success" path="protocol.message" />
			<forward name="input"
				path="/updateProtocol.do?dispatch=setupUpdate&amp;page=0" />
			<forward name="failure"
				path="/updateProtocol.do?dispatch=setupUpdate" />
			<forward name="setup" path="protocol.updateProtocol" />
		</action>
		<action input="input" name="searchProtocolForm"
			parameter="dispatch" path="/searchProtocol"
			type="gov.nih.nci.calab.ui.protocol.SearchProtocolAction"
			scope="request">
			<exception
				handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler"
				key="error.download.file" path="home.error"
				type="gov.nih.nci.calab.exception.CalabException" />
			<exception
				handler="gov.nih.nci.calab.ui.core.BaseExceptionHandler"
				bundle="protocol" key="error.searchProtocol"
				path="protocol.searchProtocol" type="java.lang.Exception" />
			<forward name="input" path="protocol.searchProtocol" />
			<forward name="success"
				path="protocol.searchProtocolResult" />
		</action>
		<action path="/protocolResults"
			type="gov.nih.nci.calab.ui.core.BaseForwardAction"
			parameter="protocol.searchProtocolResult" scope="request" />
	</action-mappings>
</struts-config>
