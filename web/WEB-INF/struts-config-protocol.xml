<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="submitProtocolForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
			<form-property name="protocol"
				type="gov.nih.nci.cananolab.dto.common.ProtocolBean" />
		</form-bean>
		<form-bean name="searchProtocolForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="fileTitle" type="java.lang.String" />
			<form-property name="searchLocations"
				type="java.lang.String[]" />
			<form-property name="protocolType" type="java.lang.String" />
			<form-property name="protocolName" type="java.lang.String" />
			<form-property name="protocolAbbreviation" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
		</form-bean>
	</form-beans>
	<global-exceptions>
		<exception
			handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
			bundle="protocol" key="error.protocolException"
			path="protocol.message"
			type="gov.nih.nci.cananolab.exception.ProtocolException" />
	</global-exceptions>
	<global-forwards>
		<forward name="protocolMessage" path="protocol.message" />
	</global-forwards>
	<action-mappings>
		<action path="/manageProtocol"
			type="gov.nih.nci.cananolab.ui.core.BaseForwardAction"
			parameter="protocol.manageProtocol" scope="request" />
		<action name="submitProtocolForm" parameter="dispatch"
			path="/submitProtocol"
			type="gov.nih.nci.cananolab.ui.protocol.SubmitProtocolAction"
			input="input" scope="request">
			<forward name="success" path="protocol.message" />
			<forward name="input"
				path="/submitProtocol.do?dispatch=input&amp;page=0" />
			<forward name="inputPage" path="protocol.submitProtocol" />
		</action>
		<action input="input" name="searchProtocolForm"
			parameter="dispatch" path="/searchProtocol"
			type="gov.nih.nci.cananolab.ui.protocol.SearchProtocolAction"
			scope="request">
			<forward name="input" path="protocol.searchProtocol" />
			<forward name="success"
				path="protocol.searchProtocolResult" />
		</action>
		<action path="/protocolResults"
			type="org.apache.struts.actions.ForwardAction"
			parameter="protocol.searchProtocolResult" scope="request" />
	</action-mappings>
</struts-config>
