    </definition>
    
     <!-- definitions for the run file mask page -->
    <definition name="workflow.runfilemask.content" extends="workflow.content.home">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp"/>
	    <put name="workflowContent" value="/workflow/bodyWorkflowFileMask.jsp"/>
    </definition>
    
    <definition name="workflow.runfilemask" extends="calab.home">
        <put name="calabContent" value="workflow.runfilemask.content"/>
