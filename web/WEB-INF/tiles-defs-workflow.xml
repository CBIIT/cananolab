<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.3//EN"
       "http://struts.apache.org/dtds/tiles-config_1_3.dtd">
<tiles-definitions>
	<!-- main layout definition for the workflow content page inside caLAB -->
	<definition name="workflow.content.home" path="/layout/workflowLayout.jsp">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp" />
		<putList name="inActions">
			<item value="Use Aliquot" link="initSession.do?forwardPage=useAliquot" />
			<item value="Upload Files" link="fileUpload.do?method=setup" />
			<item value="Download Files" link="fileDownload.do?method=setup" />
			<item value="Mask Files" link="fileMask.do?method=setup" />
		</putList>
		<putList name="outActions">
			<item value="Upload Files" link="fileUpload.do?method=setup" />
			<item value="Download Files" link="fileDownload.do?method=setup" />
			<item value="Mask Files" link="fileMask.do?method=setup" />
		</putList>
		<putList name="assayActions">
			<item value="Create Run" link="initSession.do?forwardPage=createAssayRun" />
		</putList>
	</definition>

	<!-- definitions for the use aliquot page -->
	<definition name="workflow.usealiquot.content" extends="workflow.content.home">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?type=in" />
		<put name="workflowContent" value="/workflow/bodyUseAliquot.jsp" />
	</definition>

	<definition name="workflow.usealiquot" extends="calab.home">
		<put name="calabContent" value="workflow.usealiquot.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
		<put name="menu" value="workflow" />
	</definition>

	<!-- definitions for the workflow message page -->
	<definition name="workflow.message.content" extends="workflow.content.home">
		<put name="workflowContent" value="/bodyMessage.jsp?bundle=workflow" />
	</definition>
	<definition name="workflow.message" extends="calab.home">
		<put name="calabContent" value="workflow.message.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
		<put name="menu" value="workflow" />
	</definition>

	<!-- definitions for the view aliquot page -->
	<definition name="workflow.viewaliquot.content" extends="workflow.content.home">
		<put name="workflowContent" value="/workflow/bodyViewAliquot.jsp" />
	</definition>

	<definition name="workflow.viewaliquot" extends="calab.home">
		<put name="calabContent" value="workflow.viewaliquot.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
		<put name="menu" value="workflow" />
	</definition>

	<!-- definitions for the mask aliquot page -->
	<definition name="workflow.maskaliquot.content" extends="workflow.content.home">
		<put name="workflowContent" value="/workflow/bodyMaskAliquot.jsp" />
	</definition>

	<definition name="workflow.maskaliquot" extends="calab.home">
		<put name="calabContent" value="workflow.maskaliquot.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
		<put name="menu" value="workflow" />
	</definition>
	
    <!-- definitions for the create run page -->    
    <definition name="workflow.createrun.content" extends="workflow.content.home">
 		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?type=assay"/>
        <put name="workflowContent" value="/workflow/bodyCreateRun.jsp"/>
    </definition>
    
    <definition name="workflow.createrun" extends="calab.home">
        <put name="calabContent" value="workflow.createrun.content"/>
        <put name="calabSidemenu" value="/workflow/sideMenuTree.jsp"/>
        <put name="menu" value="workflow"/>
    </definition>
    <!-- definitions for the create run page -->    
    <definition name="workflow.createassayrun.content" extends="workflow.content.home">
 		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?type=assay"/>
        <put name="workflowContent" value="/workflow/bodyCreateAssayRun.jsp"/>
    </definition>
    
    <definition name="workflow.createassayrun" extends="calab.home">
        <put name="calabContent" value="workflow.createassayrun.content"/>
        <put name="calabSidemenu" value="/workflow/sideMenuTree.jsp"/>
        <put name="menu" value="workflow"/>
    </definition>
   
   <!-- definition for the edit run page -->
    <definition name="workflow.editrun.content" extends="workflow.content.home">
 		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?type=assay"/>
        <put name="workflowContent" value="/workflow/bodyEditRun.jsp"/>
    </definition>    
	<definition name="workflow.editrun" extends="calab.home">
        <put name="calabContent" value="workflow.createrun.content"/>
        <put name="calabSidemenu" value="/workflow/sideMenuTree.jsp"/>
	</definition>   
	    
    <!-- definitions for the file upload page -->
    <definition name="workflow.fileupload.content" extends="workflow.content.home">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp"/>
	    <put name="workflowContent" value="/workflow/bodyWorkflowFileUpload.jsp"/>
    </definition>
    
     <!-- definitions for the file upload page -->
    <definition name="workflow.fileDownload.content" extends="workflow.content.home">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp"/>
	    <put name="workflowContent" value="/workflow/bodyWorkflowFiledownload.jsp"/>
    </definition>
       
    <definition name="workflow.fileUpload" extends="calab.home">
        <put name="calabContent" value="workflow.fileupload.content"/>
        <put name="calabSidemenu" value="/workflow/sideMenuTree.jsp"/>
        <put name="menu" value="workflow"/>
    </definition>

    <definition name="workflow.fileDownload" extends="calab.home">
        <put name="calabContent" value="workflow.fileDownload.content"/>
        <put name="calabSidemenu" value="/workflow/sideMenuTree.jsp"/>
        <put name="menu" value="workflow"/>
    </definition>
    
</tiles-definitions>
