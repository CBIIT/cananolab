        <put name="menu" value="workflow"/>
    </definition>
    
   	<!-- definitions for the mask file page -->
	<definition name="workflow.maskfile.content" extends="workflow.content.home">
		<put name="workflowContent" value="/workflow/bodyMaskFile.jsp" />
	</definition>

	<definition name="workflow.maskfile" extends="calab.home">
		<put name="calabContent" value="workflow.maskfile.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
