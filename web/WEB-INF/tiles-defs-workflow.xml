<tiles-definitions>
	<!-- main layout definition for the workflow content page inside caLAB -->
	<definition name="workflow.content.home" path="/layout/workflowLayout.jsp">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp" />
		<putList name="inActions">
			<item value="Use Aliquot" link="initSession.do?forwardPage=useAliquot" />
			<item value="Upload Files" link="fileUpload.do?method=setup" />
			<item value="Download All Files (Zipped)" link="useAliquot.jsp" />
			<item value="Download All Files (Unzipped)" link="useAliquot.jsp" />
		</putList>
		<putList name="outActions">
			<item value="Upload Files" link="useAliquot.jsp" />
			<item value="Download All Files (Zipped)" link="useAliquot.jsp" />
		</putList>
		<putList name="assayActions">
			<item value="Create Run" link="initSession.do?forwardPage=createRun" />
		</putList>
	</definition>

	<!-- definitions for the use aliquot page -->
	<definition name="workflow.usealiquot.content" extends="workflow.content.home">
		<put name="workflowActionMenu" value="/workflow/actionMenu.jsp?type=in" />
		<put name="workflowContent" value="/workflow/bodyUseAliquot.jsp" />
	</definition>

	<definition name="workflow.usealiquot" extends="calab.home">
		<put name="calabContent" value="workflow.usealiquot.content" />
		<put name="calabSidemenu" value="/workflow/sideMenuTree.jsp" />
		<put name="menu" value="workflow" />
	</definition>

