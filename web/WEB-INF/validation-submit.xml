<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="nanoparticleGeneralInfoForm">
			<field property="particleType" page="1" depends="required">
				<arg position="0" key="nanoparticleGeneralInfoForm.particleType" bundle="submit" />
			</field>
			<field property="particleName" page="1" depends="required">
				<arg position="0" key="nanoparticleGeneralInfoForm.particleID" bundle="submit" />
			</field>
			<!--
				<field property="visibilities" page="1" depends="required">
				<arg position="0" key="nanoparticleGeneralInfoForm.visibilities" bundle="submit" />
				</field>
			-->
		</form>

		<form name="publishReportForm">
			<field property="particleNames" page="2" depends="required">
				<arg position="0" key="publishReportForm.particleID" bundle="submit" />
			</field>
			<field property="file.type" page="2" depends="required">
				<arg position="0" key="publishReportForm.reportType" bundle="submit" />
			</field>
			<field property="uploadedFile" page="2" depends="required">
				<arg position="0" key="publishReportForm.reportFile" bundle="submit" />
			</field>
			<field property="file.title" page="1" depends="required">
				<arg position="0" key="publishReportForm.title" bundle="submit" />
			</field>
			<!--
				<field property="visibilities" page="1" depends="required">
				<arg position="0" key="publishReportForm.visibilities" bundle="submit" />
				</field>
			-->
		</form>
		<form name="submitProtocolForm">
			<field property="protocolId" page="1" depends="required">
				<arg position="0" key="submitProtocolForm.protocolName" bundle="submit" />
			</field>
			<field property="protocolType" page="1" depends="required">
				<arg position="0" key="submitProtocolForm.protocolType" bundle="submit" />
			</field>
			<field property="file.id" page="1" depends="required">
				<arg position="0" key="submitProtocolForm.protocolVersion" bundle="submit" />
			</field>
		</form>
		<form name="nanoparticleCompositionForm">
			<field property="characterizationSource" page="2" depends="required">
				<arg position="0" key="characterizationSource" bundle="submit" />
			</field>
			<field property="viewTitle" page="2" depends="required">
				<arg position="0" key="viewTitle" bundle="submit" />
			</field>
			<field property="carbonNanotube.growthDiameter" page="2" depends="float">
				<arg position="0" key="nanoparticleCompositionForm.carbonNanotube.growthDiameter" bundle="submit" />
			</field>
			<field property="carbonNanotube.averageLength" page="2" depends="float">
				<arg position="0" key="nanoparticleCompositionForm.carbonNanotube.averageLength" bundle="submit" />
			</field>
			<field property="fullerene.numberOfCarbons" page="2" depends="integer">
				<arg position="0" key="nanoparticleCompositionForm.fullerene.numberOfCarbon" bundle="submit" />
			</field>
			<field property="liposome.polymerName" page="2" depends="validwhen">
				<arg position="0" key="nanoparticleCompositionForm.liposome.polymerName" bundle="submit" />
				<var>
					<var-name>test</var-name>
					<var-value>((liposome.polymerized=="No") or (*this*!=null))</var-value>
				</var>
			</field>
			<field property="emulsion.polymerName" page="2" depends="validwhen">
				<arg position="0" key="nanoparticleCompositionForm.emulsion.polymerName" bundle="submit" />
				<var>
					<var-name>test</var-name>
					<var-value>((emulsion.polymerized=="No") or (*this*!=null))</var-value>
				</var>
			</field>
			<field property="polymer.crosslinkDegree" page="2" depends="validwhen">
				<arg position="0" key="nanoparticleCompositionForm.polymer.crosslinkDegree" bundle="submit" />
				<var>
					<var-name>test</var-name>
					<var-value>((polymer.crosslinked=="No") or (*this*!=null))</var-value>
				</var>
			</field>
		</form>

		<form name="nanoparticleFunctionForm">
			<field property="function.viewTitle" page="2" depends="required">
				<arg position="0" key="viewTitle" bundle="submit" />
			</field>
			<field property="function.numberOfLinkages" page="1" depends="integer">
				<arg position="0" key="function.numberOfLinkages" bundle="submit" />
			</field>
			<field property="agent.numberOfAgentTargets" page="1" depends="integer" indexedListProperty="function.linkages">
				<arg position="0" key="function.numberOfAgentTargets" bundle="submit" />
			</field>
		</form>
		<form name="loadDerivedBioAssayDataForm">
			<field property="uploadedFile" page="2" depends="validwhen">
				<arg position="0" key="loadDerivedBioAssayDataForm.file" bundle="submit" />
				<var>
					<var-name>test</var-name>
					<var-value>((fileSource!="new") or (*this*!=null))</var-value>
				</var>
			</field>
			<field property="file.title" page="1" depends="required">
				<arg position="0" key="loadDerivedBioAssayDataForm.title" bundle="submit" />
			</field>
		</form>	
		<form name="nanoparticleCharacterizationForm">
			<field property="achar.characterizationSource" page="2" depends="required">
				<arg position="0" key="characterizationSource" bundle="submit" />
			</field>
			<field property="achar.viewTitle" page="2" depends="required">
				<arg position="0" key="viewTitle" bundle="submit" />
			</field>
			<field property="achar.numberOfDerivedBioAssayData" page="1" depends="integer">
				<arg position="0" key="numberOfDerivedBioAssayData" bundle="submit" />
			</field>
			<field property="numberOfDataPoints" page="1" depends="integer" indexedListProperty="achar.derivedBioAssayDataList">
				<arg position="0" key="numberOfDataPoints" bundle="submit" />
			</field>
			<field property="shape.type" page="2" depends="required">
				<arg position="0" key="shape.type" bundle="submit" />
			</field>
			<field property="shape.maxDimension" page="2" depends="float">
				<arg position="0" key="shape.maxDimension" bundle="submit" />
			</field>
			<field property="shape.minDimension" page="2" depends="float">
				<arg position="0" key="shape.minDimension" bundle="submit" />
			</field>
			<field property="surface.surfaceArea" page="2" depends="float">
				<arg position="0" key="surface.surfaceAreaValue" bundle="submit" />
			</field>
			<field property="surface.zetaPotential" page="2" depends="float">
				<arg position="0" key="surface.zetaPotentialValue" bundle="submit" />
			</field>
			<field property="surface.charge" page="2" depends="float">
				<arg position="0" key="surface.chargeValue" bundle="submit" />
			</field>
			<field property="surface.numberOfSurfaceChemistries" page="1" depends="integer">
				<arg position="0" key="surface.numberOfSurfaceChemistries" bundle="submit" />
			</field>
			<field property="numberOfMolecules" page="1" depends="integer" indexedListProperty="surface.surfaceChemistries">
				<arg position="0" key="surface.numberOfMolecules" bundle="submit" />
			</field>
			<field property="solubility.criticalConcentration" page="2" depends="float">
				<arg position="0" key="solubility.criticalConcentrationValue" bundle="submit" />
			</field>
		</form>
	</formset>
</form-validation>
