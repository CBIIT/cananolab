<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="loadAliquotsForm"
			type="org.apache.struts.action.DynaActionForm">
		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/loadAliquots" name="loadAliquotsForm"
			type="gov.nih.nci.calab.ui.workflow.LoadAliquotsAction"
			scope="request" validate="false">
			<forward name="success" path="workflow.usealiquot"></forward>
			<forward name="failure" path="workflow.error"></forward>
		</action>	
	</action-mappings>
</struts-config>
