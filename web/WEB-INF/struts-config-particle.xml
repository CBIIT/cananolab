<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="nanoparticleSampleForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
			<form-property name="particleSampleBean"
				type="gov.nih.nci.cananolab.dto.particle.ParticleBean" />
		</form-bean>
		<form-bean name="searchNanoparticleSampleForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="particleSources"
				type="java.lang.String[]" />
			<form-property name="nanoparticleEntityTypes"
				type="java.lang.String[]" />
			<form-property name="functionalizingEntityTypes"
				type="java.lang.String[]" />
			<form-property name="functionTypes"
				type="java.lang.String[]" />
			<form-property name="characterizationType"
				type="java.lang.String" />
			<form-property name="characterizations"
				type="java.lang.String[]" />
			<form-property name="text" type="java.lang.String" />
			<form-property name="searchLocations"
				type="java.lang.String[]" />
			<form-property name="page" type="java.lang.Integer"
				initial="0" />
		</form-bean>

		<form-bean name="nanoparticleEntityForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="entity"
				type="gov.nih.nci.cananolab.dto.particle.composition.NanoparticleEntityBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="otherParticles"
				type="java.lang.String[]" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="functionalizingEntityForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="entity"
				type="gov.nih.nci.cananolab.dto.particle.composition.FunctionalizingEntityBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="otherParticles"
				type="java.lang.String[]" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="chemicalAssociationForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="assoc"
				type="gov.nih.nci.cananolab.dto.particle.composition.ChemicalAssociationBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="physicalCharacterizationForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="achar"
				type="gov.nih.nci.cananolab.dto.particle.characterization.PhysicalCharacterizationBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="otherParticles"
				type="java.lang.String[]" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
			<form-property name="copyData" type="java.lang.Boolean"
				initial="false" />
		</form-bean>
		<form-bean name="compositionFileForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="compFile"
				type="gov.nih.nci.cananolab.dto.common.LabFileBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="invitroCharacterizationForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="achar"
				type="gov.nih.nci.cananolab.dto.particle.characterization.InvitroCharacterizationBean" />
			<form-property name="particleId" type="java.lang.String" />
			<form-property name="otherParticles"
				type="java.lang.String[]" />
			<form-property name="idsToDelete" type="java.lang.String[]" />
			<form-property name="copyData" type="java.lang.Boolean"
				initial="false" />
		</form-bean>
	</form-beans>
	<global-exceptions>
		<exception
			handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
			key="error.composition" path="particle.annotationMessage"
			type="gov.nih.nci.cananolab.exception.ParticleCompositionException" />
		<exception
			handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
			key="error.particle" path="particle.annotationMessage"
			type="gov.nih.nci.cananolab.exception.ParticleException" />
		<exception
			handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
			key="error.characterization" path="particle.annotationMessage"
			type="gov.nih.nci.cananolab.exception.ParticleCharacterizationException" />
	</global-exceptions>
	<global-forwards>
		<forward name="particleMessage"
			path="particle.annotationMessage" />
		<forward name="detailPrintView"
			path="/particle/characterization/bodyCharacterizationDetailPrintView.jsp" />
		<forward name="summaryPrintView"
			path="/particle/characterization/bodyCharacterizationSummaryPrintView.jsp" />
		<forward name="summaryPrintAllView"
			path="/particle/characterization/bodyCharacterizationSummaryPrintAllView.jsp" />
		<forward name="annotationDeleteView"
			path="particle.annotationDeleteView" />
	</global-forwards>
	<action-mappings>
		<action path="/manageParticle"
			type="gov.nih.nci.cananolab.ui.core.BaseForwardAction"
			parameter="particle.manageNanoparticle" scope="request" />
		<action name="nanoparticleSampleForm" parameter="dispatch"
			path="/submitNanoparticleSample"
			type="gov.nih.nci.cananolab.ui.particle.SubmitNanoparticleAction"
			input="input" scope="request">
			<exception
				handler="gov.nih.nci.cananolab.ui.core.BaseExceptionHandler"
				key="error.duplicateNanoparticleSample"
				path="particle.submitNanoparticleSample"
				type="gov.nih.nci.cananolab.exception.DuplicateEntriesException" />
			<forward name="view" path="particle.generalInfo" />
			<forward name="update"
				path="particle.updateNanoparticleSample" />
			<forward name="input"
				path="particle.submitNanoparticleSample" />
		</action>
		<action input="input" name="searchNanoparticleSampleForm"
			parameter="dispatch" path="/searchNanoparticle"
			type="gov.nih.nci.cananolab.ui.particle.SearchNanoparticleAction"
			scope="request">
			<forward name="input" path="particle.searchNanoparticle" />
			<forward name="success"
				path="particle.searchNanoparticleResult" />
		</action>
		<action input="input" name="nanoparticleEntityForm"
			parameter="dispatch" path="/nanoparticleEntity"
			type="gov.nih.nci.cananolab.ui.particle.NanoparticleEntityAction">
			<forward name="input"
				path="particle.composition.particleEntity" />
			<forward name="success" path="particle.annotationMessage" />
		</action>
		<action input="input" name="functionalizingEntityForm"
			parameter="dispatch" path="/functionalizingEntity"
			type="gov.nih.nci.cananolab.ui.particle.FunctionalizingEntityAction">
			<forward name="input"
				path="particle.composition.functionalizingEntity" />
			<forward name="success" path="particle.annotationMessage" />
		</action>
		<action input="input" name="chemicalAssociationForm"
			parameter="dispatch" path="/chemicalAssociation"
			type="gov.nih.nci.cananolab.ui.particle.ChemicalAssociationAction">
			<forward name="input"
				path="particle.composition.chemicalAssociation" />
			<forward name="success" path="particle.annotationMessage" />
		</action>
		<action input="input" name="physicalCharacterizationForm"
			parameter="dispatch" path="/physicalCharacterization"
			type="gov.nih.nci.cananolab.ui.particle.PhysicalCharacterizationAction">
			<forward name="input"
				path="particle.physicalCharacterization" />
			<forward name="success" path="particle.annotationMessage" />
			<forward name="detailView"
				path="particle.physicalDetailView" />
			<forward name="summaryView"
				path="particle.physicalSummaryView" />
		</action>
		<action input="input" name="invitroCharacterizationForm"
			parameter="dispatch" path="/invitroCharacterization"
			type="gov.nih.nci.cananolab.ui.particle.InvitroCharacterizationAction">
			<forward name="input"
				path="particle.invitroCharacterization" />
			<forward name="success" path="particle.annotationMessage" />
			<forward name="detailView"
				path="particle.invitroDetailView" />
			<forward name="summaryView"
				path="particle.invitroSummaryView" />
		</action>
	</action-mappings>
</struts-config>
