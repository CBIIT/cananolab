<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">
<dwr>
	<allow>
		<create creator="new" javascript="CompositionManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.particle.DWRCompositionManager" />
			<include method="getEntityIncludePage" />
			<include method="getComposingElementTypeOptions" />
			<include method="getBiopolymerTypeOptions" />
			<include method="getAntibodySpeciesOptions" />
			<include method="getAntibodyTypeOptions" />
			<include method="getAntibodyIsotypeOptions" />
			<include method="getWallTypeOptions" />
			<include method="getCarbonNanotubeDiameterUnitOptions" />
			<include method="getCarbonNanotubeAverageLengthUnitOptions" />
			<include method="getFullereneAverageDiameterUnitOptions" />
			<include method="getAssociatedElementOptions" />
			<include method="getAssociatedComposingElements" />
		</create>
		<create creator="new" javascript="ExperimentConfigManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.particle.DWRExperimentConfigManager" />
			<include method="findExperimentConfigById" />
			<include method="findTechniqueByType" />
			<include method="findInstrumentTypesByTechniqueType" />
			<include method="findInstrumentTypesByConfigId" />
			<include method="getAllManufacturers" />
			<include method="resetExperimentConfig" />
			<include method="addInstrument" />
			<include method="deleteInstrument" />
		</create>
		<create creator="new" javascript="DataSetManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.particle.DWRCharacterizationResultManager"/>
			<include method="addColumnHeader" />
			<include method="resetDataSet" />
			<include method="addRow" />
			<include method="deleteRow" />
			<include method="findDataSetById" />
		</create>
		<create creator="new" javascript="CharacterizationManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.particle.DWRCharacterizationManager" />
			<include method="getCharacterizationOptions" />
			<include method="getAssayEndpointOptions"/>
		</create>
		<create creator="new" javascript="NanoparticleSampleManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.particle.DWRSampleManager" />
			<include method="removeOrgVisibility" />
			<include method="removeOrgNameVisibility" />
			<include method="getNanoparticleEntityTypes" />
			<include method="getFunctionalizingEntityTypes" />
			<include method="getFunctionTypes" />
		</create>
		<create creator="new" javascript="ProtocolManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.protocol.DWRProtocolManager" />
			<include method="getProtocolFiles" />
			<include method="getProtocolNames" />
			<include method="getProtocolFileUriById" />
			<include method="getProtocolFileNameById" />
			<include method="getProtocolFileVersionById" />
			<include method="getProtocolTypes" />
		</create>
		<create creator="new" javascript="PublicationManager"
			scope="session">
			<param name="class"
				value="gov.nih.nci.cananolab.ui.publication.DWRPublicationManager" />
			<include method="getPublicationCategories" />
			<include method="getPublicationStatuses" />
		</create>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.util.DataLinkBean">
			<param name="include" value="dataId, dataDisplayType" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.particle.composition.ComposingElementBean">
			<param name="include"
				value="displayName, domainComposingElementId" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.common.ProtocolFileBean">
			<param name="include"
				value="domainFileId, domainFileUri, domainFileName, domainFileVersion" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.common.ExperimentConfigBean">
			<param name="include" value="domain, instruments" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.ExperimentConfig">
			<param name="include"
				value="id, description, technique, instrumentCollection" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.Technique">
			<param name="include" value="type, abbreviation" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.Instrument">
			<param name="include"
				value="id, type, manufacturer, modelName" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.common.DataColumnBean">
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.Datum">
			<param name="include"
				value="id, name, value, valueType, valueUnit, dataRow, dataSet" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.Condition">
			<param name="include"
				value="id, name, value, valueType, valueUnit, property, datumCollection" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.common.DataSetBean">
			<param name="include" value="theDataRow, dataRows,domain, columnBeans" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.dto.common.DataRowBean">
			<param name="include" value="data, conditions, domain" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.DataRow">
			<param name="include" value="id" />
		</convert>
		<convert converter="bean"
			match="gov.nih.nci.cananolab.domain.common.DataSet">
			<param name="include" value="id" />
		</convert>
	</allow>
</dwr>
