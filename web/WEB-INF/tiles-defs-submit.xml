<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
       "http://struts.apache.org/dtds/tiles-config_1_1.dtd">
<tiles-definitions>
	<!-- definition for the manage nanoparticle page -->
	<definition name="submit.managenanoparticle" extends="calab.home">
		<put name="calabContent" value="/bodyManageItem.jsp" />
		<put name="menu" value="submit" />
		<put name="item" value="Nanoparticle" />
		<put name="itemDescription" value="/submit/bodyNanoparticleDescription.jsp" />
		<put name="createLink" value="createNanoparticle.do?dispatch=setup&amp;page=0" />
		<put name="searchLink" value="searchNanoparticle.do?dispatch=setup" />
	</definition>

	<!-- definition for the create nanoparticle page -->
	<definition name="submit.createnanoparticle" extends="calab.home">
		<put name="calabContent" value="/submit/bodyCreateNanoparticle.jsp" />
		<put name="menu" value="submit" />
	</definition>

	<definition name="submit.message" extends="calab.home">
		<put name="calabSidemenu" value="/submit/sideParticleMenu.jsp" />
		<put name="calabContent" value="/bodyMessage.jsp?bundle=submit" />
		<put name="menu" value="submit" />
	</definition>

	<!-- definition for the add nanoparticle intrinsic properties page -->
	<definition name="submit.addparticle.properties" extends="calab.home">
		<put name="calabSidemenu" value="/submit/sideParticleMenu.jsp" />
		<put name="calabContent" value="/submit/bodyParticleProperties.jsp" />
		<put name="menu" value="submit" />
	</definition>
	
	<!-- definition for the submit report page -->
	<definition name="submit.submitreport" extends="calab.home">
		<put name="calabContent" value="/submit/bodySubmitReport.jsp" />
		<put name="menu" value="submit" />
	</definition>

	<!-- definition for the add assay result page -->
	<definition name="submit.addassayresult" extends="calab.home">
		<put name="calabSidemenu" value="/submit/sideParticleMenu.jsp" />
		<put name="calabContent" value="/submit/bodyAddAssayResult.jsp" />
		<put name="menu" value="submit" />
	</definition>
</tiles-definitions>
