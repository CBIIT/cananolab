<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.3//EN"
       "http://struts.apache.org/dtds/tiles-config_1_3.dtd">

<tiles-definitions>
	<!--  Main layout definition for the home page of caLAB -->
	<definition name="calab.home" path="/layout/mainLayout.jsp">
		<put name="title" value="caLAB Home" />
		<put name="nciHeader" value="/html/nciHeader.html" />
		<put name="calabHeader" value="/html/calabHeader.html" />
		<put name="calabSidemenu" value="/html/calabBaseSidemenu.html" />
		<put name="calabMainmenu" value="/html/calabMainmenu.html" />
		<put name="calabContent" value="/bodyLogin.jsp" />
		<put name="calabFooter" value="/html/calabFooter.html" />
		<put name="nciFooter" value="/html/nciFooter.html" />
	</definition>
	<definition name="calab.error" extends="calab.home">
		<put name="calabContent" value="/bodyError.jsp" />
	</definition>
</tiles-definitions>
