<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.common" auto-import="false">
	<class name="PointOfContact" table="point_of_contact" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="poc_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the PointOfContact class -->
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="email" type="string" column="email"/>
		<property name="firstName" type="string" column="first_name"/>
		<property name="lastName" type="string" column="last_name"/>
		<property name="middleInitial" type="string" column="middle_initial"/>
		<property name="phone" type="string" column="phone"/>
		<property name="role" type="string" column="role"/>
		<!-- Associations mapping for the PointOfContact class -->
		<many-to-one name="organization" class="gov.nih.nci.cananolab.domain.common.Organization" column="organization_pk_id" lazy="proxy" />
		<set name="sampleCollectionOtherPOC" lazy="true">
			<cache usage="read-write" />
			<key column="primary_contact_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Sample"/>
		</set>
		<set name="sampleCollection" table="sample_other_poc" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="poc_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Sample" column="sample_pk_id" />
		</set>
		<set name="characterizationCollection" lazy="true">
			<cache usage="read-write" />
			<key column="poc_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Characterization"/>
		</set>
		<set name="studyCollectionOtherPOC" table="study_other_poc" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="poc_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Study" column="study_pk_id" />
		</set>
		<set name="studyCollection" lazy="true">
			<cache usage="read-write" />
			<key column="primary_poc_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.common.Study"/>
		</set>
	</class>
</hibernate-mapping>
