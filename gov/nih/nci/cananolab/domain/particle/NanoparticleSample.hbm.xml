<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.particle" auto-import="false">
	<class name="NanoparticleSample" table="nanoparticle_sample" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="particle_sample_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the NanoparticleSample class -->
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="name" type="string" column="particle_sample_name"/>
		<!-- Associations mapping for the NanoparticleSample class -->
		<set name="characterizationCollection" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="particle_sample_pk_id" />
			<one-to-many class="gov.nih.nci.cananolab.domain.particle.characterization.Characterization"/>
		</set>
		<set name="keywordCollection" table="keyword_nanoparticle_sample" lazy="false" inverse="false" cascade="save-update">
			<cache usage="read-write" />
			<key column="particle_sample_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Keyword" column="keyword_pk_id" />
		</set>
		<one-to-one name="sampleManagement" class="gov.nih.nci.cananolab.domain.common.SampleManagement" property-ref="nanoparticleSample" />
		<set name="reportCollection" table="nanoparticle_sample_report" lazy="true" inverse="true">
			<cache usage="read-write" />
			<key column="particle_sample_pk_id" />
			<many-to-many class="gov.nih.nci.cananolab.domain.common.Report" column="file_pk_id" />
		</set>
		<many-to-one name="source" class="gov.nih.nci.cananolab.domain.common.Source" column="source_pk_id" lazy="proxy" cascade="save-update"/>
		<one-to-one name="sampleComposition" class="gov.nih.nci.cananolab.domain.particle.samplecomposition.SampleComposition" property-ref="nanoparticleSample" />
	</class>
</hibernate-mapping>
