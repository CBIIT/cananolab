<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.cananolab.domain.particle.characterization.physical" auto-import="false">
	<class name="SurfaceChemistry" table="surface_chemistry" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="surface_chemistry_pk_id">
			<generator class="hilo">
                          <param name="table">hibernate_unique_key</param>
                          <param name="column">next_hi</param>
                        </generator>
		</id>
		<!-- Attributes mapping for the SurfaceChemistry class -->
		<property name="createdBy" type="string" column="created_by"/>
		<property name="createdDate" type="java.util.Date" column="created_date"/>
		<property name="molecularFormulaType" type="string" column="molecular_formula_type"/>
		<property name="molecularFormula" type="string" column="molecular_formula"/>
		<property name="moleculeName" type="string" column="molecule_name"/>
		<property name="numberOfMolecule" type="integer" column="number_molecule"/>
		<!-- Associations mapping for the SurfaceChemistry class -->
	</class>
</hibernate-mapping>
