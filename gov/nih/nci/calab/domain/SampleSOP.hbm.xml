<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="SampleSOP" table="sample_sop" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="sample_sop_pk_id" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="description" type="java.lang.String" column="description" />
    <property name="name" type="java.lang.String" column="sop_name" />
    <set name="sampleSOPFileCollection" cascade="all-delete-orphan" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="sample_sop_pk_id" />
      <one-to-many class="gov.nih.nci.calab.domain.SampleSOPFile" />
    </set>
  </class>
</hibernate-mapping>

