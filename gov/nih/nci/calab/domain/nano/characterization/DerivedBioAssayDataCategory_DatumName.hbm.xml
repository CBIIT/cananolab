<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization">
	<class name="DerivedBioAssayDataCategory" table="DEF_BIOASSAY_DATA_CATEGORY" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="CATEGORY_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="characterizationName" type="java.lang.String" column="CHARACTERIZATION_NAME" />	
		<property name="name" type="java.lang.String" column="NAME" />		
	</class>
	
	<class name="DatumName" table="DEF_DATUM_NAME" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" column="DATUM_NAME_PK_ID" unsaved-value="0">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" column="NAME" />
		<property name="datumParsed" type="java.lang.Boolean" column="IS_DATUM_PARSED"/>
		<property name="characterizationName" type="java.lang.String" column="CHARACTERIZATION_NAME"/>
	</class>	
</hibernate-mapping>
