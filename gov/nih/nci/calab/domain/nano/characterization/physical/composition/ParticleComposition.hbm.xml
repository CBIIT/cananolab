<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.characterization.physical.composition">
	<joined-subclass name="ParticleComposition" table="COMPOSITION" extends="gov.nih.nci.calab.domain.nano.characterization.Characterization">
		<key column="COMPOSITION_PK_ID" />
		<set name="composingElementCollection" cascade="all-delete-orphan" lazy="true">
			<cache usage="read-write" />
			<key column="COMPOSITION_PK_ID" />
			<one-to-many class="gov.nih.nci.calab.domain.nano.characterization.physical.composition.ComposingElement" />
		</set>
		<joined-subclass name="FullereneComposition" table="FULLERENE_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="F_COMPOSITION_PK_ID" />
			<property name="numberOfCarbon" column="NUMBER_OF_CARBON" type="java.lang.Integer" />
		</joined-subclass>
		<joined-subclass name="DendrimerComposition" table="DENDRIMER_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="D_COMPOSITION_PK_ID" />
			<property name="branch" column="BRANCH" type="java.lang.String" />
			<property name="generation" column="GENERATION" type="java.lang.Float" />
			<property name="molecularFormula" column="MOLECULAR_FORMULA" type="java.lang.String" />
			<property name="repeatUnit" column="REPEAT_UNIT" type="java.lang.String" />
			<set name="surfaceGroupCollection" cascade="all-delete-orphan" lazy="false">
				<cache usage="read-write" />
				<key column="D_COMPOSITION_PK_ID" />
				<one-to-many class="gov.nih.nci.calab.domain.nano.characterization.physical.composition.SurfaceGroup" />
			</set>
		</joined-subclass>
		<joined-subclass name="CarbonNanotubeComposition" table="CARBON_NANOTUBE_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="CN_COMPOSITION_PK_ID" />
			<property name="chirality" column="CHIRALITY" type="java.lang.Float" />
			<property name="growthDiameter" column="GROWTH_DIAMETER" type="java.lang.Float" />
			<property name="averageLength" column="AVERAGE_LENGTH" type="java.lang.Float" />
			<property name="wallType" column="WALL_TYPE" type="java.lang.String" />
		</joined-subclass>
		<joined-subclass name="LiposomeComposition" table="LIPOSOME_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="LC_COMPOSITION_PK_ID" />
			<property name="polymerized" column="IS_POLYMERIZED" type="boolean" />
			<property name="polymerName" column="POLYMER_NAME" type="java.lang.String" />
		</joined-subclass>
		<joined-subclass name="PolymerComposition" table="POLYMER_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="POLYMER_COMPOSITION_PK_ID" />
			<property name="crossLinked" column="IS_CROSS_LINK" type="boolean" />
			<property name="crossLinkDegree" column="CROSS_LINK_DEGREE" type="java.lang.Float" />
			<property name="initiator" column="INITIATOR" type="java.lang.String" />
		</joined-subclass>
		<joined-subclass name="EmulsionComposition" table="EMULSION_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="EMULSION_COMPOSITION_PK_ID" />
			<property name="type" column="EMULSION_TYPE" type="java.lang.String" />
			<property name="molecularFormula" column="MOLECULAR_FORMULA" type="java.lang.String" />
			<property name="polymerized" column="IS_POLYMERIZED" type="boolean" />
			<property name="polymerName" column="POLYMER_NAME" type="java.lang.String" />
		</joined-subclass>
		<joined-subclass name="MetalParticleComposition" table="VIEW_METAL_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="METAL_COMPOSITION_PK_ID" />
		</joined-subclass>
		<joined-subclass name="QuantumDotComposition" table="VIEW_QUANTUM_DOT_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="QUANTUM_DOT_COMPOSITION_PK_ID" />
		</joined-subclass>
		<joined-subclass name="ComplexComposition" table="COMPLEX_COMPOSITION" extends="gov.nih.nci.calab.nano.characterization.physical.compostion.ParticleComposition">
			<key column="C_COMPOSITION_PK_ID" />
				<property name="name" column="NAME" type="java.lang.String" />
		</joined-subclass>
	</joined-subclass>
</hibernate-mapping>
