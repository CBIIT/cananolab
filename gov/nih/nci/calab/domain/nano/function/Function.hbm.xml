<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain.nano.function">
  <class name="Function" table="Function" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="FUNCTION_PK_ID" unsaved-value="0">
      <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    
    <property name="functionType" type="java.lang.String" column="FUNCTION_TYPE" />
    <property name="activationMethod" type="java.lang.String" column="ACTIVATION_METHOD" />
       
    <many-to-one name="nanoParticle"  class="gov.nih.nci.calab.domain.nano.particle.Nanoparticle" column="PARTICLE_PK_ID" lazy="false" fetch="join"/>
  </class>
</hibernate-mapping>


