<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Project" table="Project" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="PROJECT_PK_ID">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="PROJECT_NAME" />
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <set name="sampleCollection" table="Project_sample" cascade="all-delete-orphan" lazy="true">
      <cache usage="read-write" />
      <key column="PROJECT_PK_ID" />
      <many-to-many class="gov.nih.nci.calab.domain.Sample" column="SAMPLE_PK_ID" />
    </set>
  </class>
</hibernate-mapping>

