<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="SampleType" table="Sample_Type" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="SAMPLE_TYPE_PK_ID" unsaved-value="0">
      <generator class="assigned" />
    </id>
    <property name="name" type="java.lang.String" column="SAMPLE_TYPE_NAME" />
  </class>
</hibernate-mapping>