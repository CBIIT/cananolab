<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.calab.domain">
  <class name="Keyword" table="Keyword" lazy="true" polymorphism="explicit" >
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="KEYWORD_PK_ID">
            <generator class="org.hibernate.id.TableHiLoGenerator">
  	  			<param name="table">hibernate_unique_key</param>
				<param name="column">next_hi</param>
	   </generator>
    </id>
    <property name="name" type="java.lang.String" column="NAME" />
    
	<set name="nanoparticleCollection" table="KEYWORD_NANOPARTICLE" cascade="all-delete-orphan" lazy="true" inverse="true">
      			<cache usage="read-write" />
      			<key column="KEYWORD_PK_ID" />
		      	<many-to-many class="gov.nih.nci.calab.domain.nano.particle.Nanoparticle" column="NANOPARTICLE_PK_ID"/>
	</set>  
  </class>
</hibernate-mapping>

